==============================================================================
Generates a build task optionset.

  ${config-optionset-name}    Specifies which config set to use for creating build settings
  ${build-optionset-name}     Specifies the name of the build taks optionset to create
    
==============================================================================
-->

<project>

  <createtask name="GenerateBuildOptionset">
    <parameters>
      <option name="configsetname" />
    </parameters>
    <code>
            <GenerateBuildOptionset configsetname="${GenerateBuildOptionset.configsetname}"/>
    </code>
  </createtask>

</project>
