<project>
  <property name="config-name" value="dev-debug"/>
  <property name="config-type" value="deprecated-d"/>

  <optionset name="config-options-common">
    <option name="debugflags"              value="on" />
    <option name="incrementallinking"                   value="on" />
    <option name="iteratorboundschecking"               value="on" />
    <option name="optimization"              value="off" />
    <option name="runtimeerrorchecking"          value="on" />
    <option name="usedebuglibs"              value="on" />
  </optionset>

  <optionset name="config-options-windowsprogram" append="true">
    <option name="multithreadeddynamiclib"        value="on" />
  </optionset>
  
  <optionset name="config-options-library" append="true">
    <option name="multithreadeddynamiclib"        value="on" />
  </optionset>

</project>
