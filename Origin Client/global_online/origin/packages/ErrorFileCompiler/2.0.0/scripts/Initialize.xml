<project>
  <dependent name="OriginPython"/>
  <createtask name="ErrorFileCompiler.generate">
    <parameters>
      <option name="filename" value="required"/>
      <option name="output_dir" value="required"/>
    </parameters>
    <code>
      <mkdir dir="${ErrorFileCompiler.generate.output_dir}"/>
      <task name="OriginPython.exec" script="${package.ErrorFileCompiler.dir}/source/ErrorFileCompiler.py" args="${ErrorFileCompiler.generate.filename} ${ErrorFileCompiler.generate.output_dir}"/>
    </code>
  </createtask>
</project>
