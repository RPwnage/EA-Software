<project>
    <fragments>
        <include name="masterconfig_shared.xml" />
    </fragments>
    
    <masterversions>
        <!-- build environment packages -->
        <package name="osx_config"                      version="1.20.01"/>
        <package name="XcodeProjectizer"                version="3.08.01"/>

        <!-- EAIO 3 version depends on job_manager which requires a minimum deployment target of 9.0 -->
        <package name="EAIO"                            version="2.24.02"/>

        <!-- shared dependent packages for test -->
        <package name="EATestPrintServer"               version="2.03.00"/>
    </masterversions>

    <buildroot>../build</buildroot>
    <ondemand>true</ondemand>
    <ondemandroot>../../packages</ondemandroot>

    <globalproperties>
        iphone-deployment-target-version=8.0
        osx-deployment-target-version=10.7
        iphone-extra-frameworks="CFNetwork Foundation Security SystemConfiguration"
        iphone-development-team="TSTV75T6Q5"
        iphone-provisioning-profile="Wildcard ROW"
        iphone-codesign-identity="iPhone Developer: EA Dev (TZ5CR628WR)"
        eaconfig.generate-portable-solution=true
        eadpsdk_init_dirtysdk=true
        xcode-application-directory="Xcode.app"
    </globalproperties>
    <globaldefines>
        EAMAIN_DISABLE_DEFAULT_NETWORK_CHANNEL
    </globaldefines>

    <config package="eaconfig" default="osx-x64-clang-dev-debug" includes="osx-x64-clang-dev-debug
                                                                           osx-x64-clang-dev-opt" />

</project>
