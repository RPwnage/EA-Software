<project>
    <target name="generation">
        <task name="EadpSdkProtoCodegenTask" InputDir="${package.dir}/proto/com/ea/eadp/antelope/rtm/protocol/" OutputDir="${package.dir}" />
    </target>
    <target name="regen">
        <task name="EadpSdkProtoCodegenResetTask" TargetDir="${package.dir}" />
        <call target="generation" />
    </target>
</project>
