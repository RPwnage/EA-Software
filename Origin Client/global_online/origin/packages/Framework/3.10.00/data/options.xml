      <rule regex="^[-/]Yu(&quot;[^&quot;]+&quot;)?$"   xmlname="UsePrecompiledHeader"        xmlvalue="2" />
      <rule regex="^[-/]Yc&quot;([^&quot;]+)&quot;$"    xmlname="PrecompiledHeaderThrough"    xmlvalue="{1}" />
      <rule regex="^[-/]YX&quot;([^&quot;]+)&quot;$"    xmlname="PrecompiledHeaderThrough"    xmlvalue="{1}" />
      <rule regex="^[-/]Yu&quot;([^&quot;]+)&quot;$"    xmlname="PrecompiledHeaderThrough"    xmlvalue="{1}" />
      <rule regex="^([-/]Gs[0-9]*)$"        xmlname="AdditionalOptions"                       xmlvalue="{1} {0}"      setonce="false"  />
      <rule regex="^[-/]GS$"                xmlname="BufferSecurityCheck"                     xmlvalue="TRUE" />
      <rule regex="^([-/]Wall)$"            xmlname="AdditionalOptions"                       xmlvalue="{1} {0}"      setonce="false"  />
      <rule regex="^[-/]GX$"                xmlname="ExceptionHandling"                       xmlvalue="1" />
      <rule regex="^[-/]EHsc$"              xmlname="ExceptionHandling"                       xmlvalue="1" />
      <rule regex="^[-/]EHa$"               xmlname="ExceptionHandling"                       xmlvalue="2" />
      <rule regex="^[-/]AI(&quot;[^&quot;]+&quot;)$"  xmlname="AdditionalUsingDirectories"    xmlvalue="{1}; {0}"     setonce="false"  />
      <rule regex="^[-/]TC$"                xmlname="CompileAs"                               xmlvalue="1" />
      <rule regex="^[-/]TP$"                xmlname="CompileAs"                               xmlvalue="2" />
      <rule regex="^([-/]FD)$"              xmlname="AdditionalOptions"                       xmlvalue="{1} {0}"      setonce="false"  />
      <rule regex="^[-/]FU (&quot;[^&quot;]+&quot;)$"  xmlname="ForcedUsingFiles"             xmlvalue="{1}; {0}"     setonce="false"  />
      <rule regex="^([-/]callcap)$"         xmlname="AdditionalOptions"                       xmlvalue="{1} {0}"      setonce="false"  />
      <rule regex="^([-/]fastcap)$"         xmlname="AdditionalOptions"                       xmlvalue="{1} {0}"      setonce="false"  />
      <rule regex="^([-/]QVMX128)$"         xmlname="AdditionalOptions"                       xmlvalue="{1} {0}"      setonce="false"  />
      <rule regex="^[-/]Wp64$"              xmlname="Detect64BitPortabilityProblems"          xmlvalue="TRUE" />
      <rule regex="^[-/]Wp64-$"             xmlname="Detect64BitPortabilityProblems"          xmlvalue="FALSE" />
      <rule regex="^([-/]FA)$"              xmlname="AssemblerOutput"                         xmlvalue="1"            setonce="true" />
      <rule regex="^([-/]FAcs)$"            xmlname="AssemblerOutput"                         xmlvalue="2"            setonce="true" />
      <rule regex="^([-/]FAc)$"             xmlname="AssemblerOutput"                         xmlvalue="3"            setonce="true" />
      <rule regex="^([-/]FAs)$"             xmlname="AssemblerOutput"                         xmlvalue="4"            setonce="true" />
      <rule regex="^([-/]G[hH])$"           xmlname="AdditionalOptions"                       xmlvalue="{1} {0} "     setonce="false"/>
