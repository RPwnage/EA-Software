      <rule regex="^[-/]fp:fast$"           xmlname="FloatingPointModel"                      xmlvalue="2"/>
      <rule regex="^[-/]FI\s*&quot;?([^&quot;]+)&quot;?$"    xmlname="ForcedIncludeFiles"     xmlvalue="{0};{1}"      ignorecase="true" setonce="false"/>
      <rule regex="^[-/]Gm$"                xmlname="MinimalRebuild"                          xmlvalue="true"/>
      <rule regex="^[-/]Gm-$"               xmlname="MinimalRebuild"                          xmlvalue="false"/>
      <rule regex="^([-/]MP[0-9]*)$"        xmlname="AdditionalOptions"                       xmlvalue="{1} {0}"      setonce="false" />
      <rule regex="^([-/]Zm[1-9][0-9][0-9][0-9]?)$"          xmlname="AdditionalOptions"      xmlvalue="{1} {0}"      setonce="false" />
      <rule regex="^([-/]bigobj)$"          xmlname="AdditionalOptions"                       xmlvalue="{1} {0}"      setonce="false"  />
      
    </vc>
    <gcc>      
      <rule regex="^-(.+)$"                 xmlname="AdditionalOptions"                       xmlvalue="{0} -{1}"    setonce="false"/>
    </gcc>
    <sn>
      <rule regex="^-o\s&quot;%objectfile%&quot;$"  xmlname=""                                xmlvalue=""             matchonce="true"/>
      <rule regex="^-c\s&quot;%sourcefile%&quot;$"  xmlname=""                                xmlvalue=""             matchonce="true"/>
      <rule regex="^-(.+)$"                 xmlname="AdditionalOptions"                       xmlvalue="{0} -{1}"     setonce="false"/>
    </sn>
    <ghs>
      <rule regex="^-(.+)$"                 xmlname="AdditionalOptions"                       xmlvalue="{0} -{1}"     setonce="false"/>
    </ghs>    
    <psp2>
      <rule regex="^-o\s&quot;%objectfile%&quot;$"  xmlname=""                                xmlvalue=""             matchonce="true"/>
      <rule regex="^-c\s&quot;%sourcefile%&quot;$"  xmlname=""                                xmlvalue=""             matchonce="true"/>
      <rule regex="^-(.+)$"                 xmlname="AdditionalOptions"                       xmlvalue="{0} -{1}"     setonce="false"/>
    </psp2>
    <mw>
      <rule regex="^-precompile &quot;([^&quot;]+)&quot;$"       xmlname="PrecompiledHeaderFile"       xmlvalue="{1}" />
      <rule regex="^-precompile (&quot;[^&quot;]+&quot;)?$"      xmlname="UsePrecompiledHeader"        xmlvalue="1" />

      <rule regex="^-(.+)$"							    xmlname="AdditionalOptions"					                        xmlvalue="{0} -{1}"		setonce="false"/>
    </mw>    
    <xenon>
      <rule regex="^[-/]openmp$"            xmlname="OpenMP"                                  xmlvalue="TRUE"   />
      <rule regex="^([-/]FastCAP)$"         xmlname="FastCAP"                                 xmlvalue="TRUE"   />
      <rule regex="^([-/]QXSTALLS)$"        xmlname="AdditionalOptions"                       xmlvalue="{1} {0}"      setonce="false"  />
      <rule regex="^[-/]Oc$"                xmlname="TrapIntegerDivides"                      xmlvalue="TRUE"/>
      <rule regex="^[-/]Ou$"                xmlname="Prescheduling"                           xmlvalue="TRUE"/>
      <rule regex="^[-/]Oz$"                xmlname="InlineAssembly"                          xmlvalue="TRUE"/>
