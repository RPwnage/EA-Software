  <Target Name="GetCorFlagsPath" BeforeTargets="NAntCreate32BitExecutable">
    <GetFrameworkSdkPath>
      <Output TaskParameter="Path" PropertyName="WindowsSdkPath" />
    </GetFrameworkSdkPath>
    <PropertyGroup>
      <CorFlagsPath>$(WindowsSdkPath)bin\corflags.exe</CorFlagsPath>
    </PropertyGroup>
    <PropertyGroup>
      <CorFlagsPath Condition="!Exists($(CorFlagsPath))">$(WindowsSdkPath)bin\NETFX 4.0 Tools\corflags.exe</CorFlagsPath>
    </PropertyGroup>
  </Target>
  <Target Name="NAntCreate32BitExecutable">
    <Message Text="Create 32 bit version of NAnt '$(OutputPath)\nant_32.exe'" Importance="high" />
    <Copy OverwriteReadOnlyFiles="true" SkipUnchangedFiles="true" SourceFiles="$(OutputPath)\nant.exe" DestinationFiles="$(OutputPath)\nant_32.exe" />
    <Copy OverwriteReadOnlyFiles="true" SkipUnchangedFiles="true" SourceFiles="$(OutputPath)\nant.exe.config" DestinationFiles="$(OutputPath)\nant_32.exe.config" />
    <Exec Command="&quot;$(CorFlagsPath)&quot; /32BIT+ &quot;$(OutputPath)\nant_32.exe&quot;" ContinueOnError="true">
      <Output TaskParameter="ExitCode" PropertyName="ErrorCode" />
    </Exec>
    <Message Text="CorFlags.exe exit code is $(ErrorCode)" />
    <Error Text="Error while executing $(CorFlagsPath)" Condition="'$(ErrorCode)' &gt; '0'" />
    <Copy OverwriteReadOnlyFiles="true" SkipUnchangedFiles="true" SourceFiles="$(OutputPath)\copy.exe" DestinationFiles="$(OutputPath)\copy_32.exe" />
    <Copy OverwriteReadOnlyFiles="true" SkipUnchangedFiles="true" SourceFiles="$(OutputPath)\mkdir.exe" DestinationFiles="$(OutputPath)\mkdir_32.exe" />
    <Exec Command="&quot;$(CorFlagsPath)&quot; /32BIT+ &quot;$(OutputPath)\copy_32.exe&quot;" ContinueOnError="true">
      <Output TaskParameter="ExitCode" PropertyName="ErrorCode" />
    </Exec>
    <Message Text="CorFlags.exe exit code is $(ErrorCode)" />
    <Error Text="Error while executing $(CorFlagsPath)" Condition="'$(ErrorCode)' &gt; '0'" />
    <Exec Command="&quot;$(CorFlagsPath)&quot; /32BIT+ &quot;$(OutputPath)\mkdir_32.exe&quot;" ContinueOnError="true">
      <Output TaskParameter="ExitCode" PropertyName="ErrorCode" />
    </Exec>
    <Message Text="CorFlags.exe exit code is $(ErrorCode)" />
    <Error Text="Error while executing $(CorFlagsPath)" Condition="'$(ErrorCode)' &gt; '0'" />
  </Target>
  <Target Name="NAntCopyToBinDir" Condition="'$(Configuration)|$(Platform)' == 'Release|AnyCPU'">
    <Message Text="Copying output files from '$(OutputPath)' to '$(OutputPath)..\..\bin'" Importance="high" />
    <ItemGroup>
      <NantReleaseFiles Include="$(OutputPath)**\*.*" />
    </ItemGroup>
    <Copy OverwriteReadOnlyFiles="true" SkipUnchangedFiles="true" SourceFiles="@(NantReleaseFiles)" DestinationFiles="@(NantReleaseFiles->'$(OutputPath)..\..\bin\%(RecursiveDir)%(Filename)%(Extension)')" />
  </Target>
  <Target Name="AfterBuild">
