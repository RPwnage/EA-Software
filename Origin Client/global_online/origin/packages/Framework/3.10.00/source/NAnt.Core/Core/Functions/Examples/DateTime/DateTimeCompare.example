<example>
<code>
<![CDATA[
<project>
    <touch file="file.txt" />
    <property name='t1' value="@{FileGetLastWriteTime('file.txt')}" />
    <property name='t2' value="@{FileGetLastWriteTime('file.txt')}" />
    
    <fail unless="@{DateTimeCompare('${t1}', '${t2}')} == 0" />
</project>
]]>
</code>
</example>