<example>
  <para>Move a set of files.</para>
  <code>
  <![CDATA[
  <project default="MoveFiles">
     <target name="MoveFiles">
        <mkdir dir="C:\test"/>
        <move todir="C:\test">
            <fileset basedir=".">
                <includes name="*.txt"/>
            </fileset>
        </move>
     </target>
  </project>
  ]]>
  </code>
</example>
