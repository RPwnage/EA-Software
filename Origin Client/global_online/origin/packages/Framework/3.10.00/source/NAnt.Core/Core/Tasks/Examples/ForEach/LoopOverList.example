<example>
  <para>Loops over a list</para>
  <code>
    <![CDATA[
    <project default="doloop">
        <target name="doloop">
            <foreach item="String" in="1 2 3" delim=" " property="count">
                <echo message="${count}"/>
            </foreach>
        </target>
    </project>
    ]]>
  </code>
</example>
