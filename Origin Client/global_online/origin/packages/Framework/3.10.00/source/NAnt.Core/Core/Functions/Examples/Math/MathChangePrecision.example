<example>
<code>
<![CDATA[
<project>
    <eval code="@{MathSubf('2.2', '1.2')}" property='p' type='Function'/>
    <fail unless='${p} == 1.0000000000000002' />

    <eval code="@{MathChangePrecision('${p}', '15')}" property='p' type='Function'/>
    <fail unless='${p} == 1' />
</project>
]]>
</code>
</example>