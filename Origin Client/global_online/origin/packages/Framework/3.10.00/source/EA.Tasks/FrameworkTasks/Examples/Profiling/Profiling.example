<example>
  <para>
  Makes task-level profiling.
  </para>
  <code>
<![CDATA[
<project>
    <package name="ProfilingTest" targetversion="1.0.0"/>
    <profilestart name="dependent1"/>
    <dependent name="VisualStudio"/>
    <profileend/>
</project>]]>
  </code>
  <code file="Manifest.xml">
<![CDATA[
<package>
    <frameworkVersion>2</frameworkVersion>
    <buildable>true</buildable>
</package>
]]>
</code>
  <code file="masterconfig.xml">
<![CDATA[
<project>
    <masterversions>
		<package name="Framework" version="dev"/>
		<package name="VisualStudio" version="7.1.1-4"/>
		<package name='DotNet' version='1.1.4322'	/>
		<package name='DotNetSDK' version='1.1-1' />
    </masterversions>
    <config package="Framework" default="win-release"/>
</project>
]]>
  </code>
</example>
