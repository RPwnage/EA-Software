<project>
    <property name="deobfuscate.dir" value="${package.OriginObfuscateStrings.dir}/Deobfuscate"/>
    <Library name="Deobfuscate">
      <config>
        <buildoptions>
          <option name="warningsaserrors" value="false"/>
        </buildoptions>
      </config>
      <dependencies>
        <interface>
          EABase
        </interface>
      </dependencies>
      <includedirs>
        ${deobfuscate.dir}/include/
      </includedirs>
      <headerfiles>
        <includes name="${deobfuscate.dir}/include/ObfuscateStrings/*.h" />
      </headerfiles>
      <sourcefiles>
        <includes name="${deobfuscate.dir}/source/*.cpp" />
      </sourcefiles>
    </Library>
</project>