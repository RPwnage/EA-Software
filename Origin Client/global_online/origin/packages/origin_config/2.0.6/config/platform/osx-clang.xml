<project>
    
  <property name="xcode-osx-template-dir" value="../dev-mac/XCodeTemplate/Generic_x86_64.template" if='${package.xcode.version} >= 10.0'/>
  <property name="xcode-extra-buildsettings" value="VALID_ARCHS=x86_64" if='${package.xcode.version} >= 10.0'/>
  <property name="xcode-use-legacy-build-system" value="true" if='${package.xcode.version} >= 10.0'/>

  <property name="xcode-osx-template-dir" value="../dev-mac/XCodeTemplate/Generic.template" if='${package.xcode.version} &lt; 10.0'/>
  <property name="xcode-extra-buildsettings" value="VALID_ARCHS=$(ARCHS_STANDARD_32_64_BIT)" if='${package.xcode.version} &lt; 10.0'/>

  <property name="origin.osx.warning.flags">
    -Wno-unused-private-field
    -Wno-inconsistent-missing-override
    -Wno-unused-local-typedefs
    -Wno-deprecated-register
    -Wno-format-security
    -Wno-unknown-pragmas
    -Wno-initializer-overrides
    -Wno-reorder
  </property>
  <optionset name="config-options-dynamiclibrary">
    <option name="buildset.cc.options">
      ${option.value}
      ${origin.osx.warning.flags}
    </option>
  </optionset>
  <optionset name="config-options-common">
    <option name="buildset.cc.options">
      ${option.value}
      ${origin.osx.warning.flags}
    </option>
  </optionset>
  <optionset name="config-options-program">
    <option name="buildset.cc.options">
      ${option.value}
      ${origin.osx.warning.flags}
    </option>
  </optionset>
  <optionset name="config-options-library">
    <option name="buildset.cc.options">
      ${option.value}
      ${origin.osx.warning.flags}
    </option>
  </optionset>

</project>


