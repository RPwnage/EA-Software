<project>
	<property name="package.OriginUIToolkit.includedirs">
		${package.OriginUIToolkit.dir}/include/Qt
		${package.OriginUIToolkit.dir}/include
		${package.OriginUIToolkit.dir}/generated/${config}
	</property>

	<property name="package.OriginUIToolkit.libdir" value="${package.OriginUIToolkit.builddir}/${config}/lib" />
	
    <fileset name="package.OriginUIToolkit.libs">
        <includes name="${package.OriginUIToolkit.libdir}/OriginUIToolkit.lib" if="${config-system} == 'pc'"/>
        <includes name="${package.OriginUIToolkit.libdir}/libuitoolkit.a" if="${config-system} == 'osx'"/>
    </fileset>
		
		<include file="targets.xml" />
	</project>