<project>
    <masterversions>
        <package name="Framework"                       version="6.04.01" />
        <package name="eaconfig"                        version="4.03.01"/>
        <package name="osx_config"                      version="1.11.00"/>
        <package name="ios_config"                      version="1.04.01"/>
        <package name="UnixGCC"                         version="0.10.00"/>
        <package name="iphonesdk"                       version="8.0-proxy-3" />
        <package name="XcodeProjectizer"                version="3.01.00" />
        <package name="EABase"                          version="2.07.01"/>
    </masterversions>

    <buildroot>build</buildroot>
    <ondemand>true</ondemand>
    <ondemandroot>../../packages</ondemandroot>

    <globalproperties>
      dirtysdk-force-stripped
      dirtysdk-distpktwindow-size
      dirtysdk-prebuilt-libs
      dirtysdk-samples
      iphone-base-sdk-version=9.2
      iphone-extra-frameworks="CFNetwork Foundation Security SystemConfiguration"
      package.iphonesdk.appdir="/Applications/Xcode.app/Contents/Developer"
      xcode-projectize
    </globalproperties>

    <config package="eaconfig" default="iphonesim-clang-dev-debug" includes="iphonesim-x64-clang-dev-* iphonesim-clang-dev-* osx-clang-dev*"/>   
</project>
