<project>
    <masterversions>
        <package name="Framework"                       version="3.32.02" />
        <package name="eaconfig"                        version="2.25.01"/>
        <package name="osx_config"                      version="1.00.01"/>
        <package name="ios_config"                      version="1.01.05"/>
        <package name="UnixGCC"                         version="0.10.00"/>
        <package name="iphonesdk"                       version="7.0-proxy-5"/>
        <package name="XcodeProjectizer"                version="3.00.01" />
    </masterversions>

    <buildroot>build</buildroot>
    <ondemand>true</ondemand>
    <ondemandroot>../../packages</ondemandroot>

    <globalproperties>
      dirtysdk-force-stripped
      dirtysdk-distpktwindow-size
      dirtysdk-prebuilt-libs
      dirtysdk-samples
      dirtysdk-namespaced-includes
      iphone-base-sdk-version=7.1
      iphone-extra-frameworks="CFNetwork Foundation Security SystemConfiguration"
      package.iphonesdk.appdir="/Applications/Xcode.app/Contents/Developer"
      xcode-projectize
    </globalproperties>

    <config package="eaconfig" default="iphonesim-clang-dev-debug" includes="iphonesim-x64-clang-dev-* iphonesim-clang-dev-* osx-gcc-dev*"/>   
</project>
