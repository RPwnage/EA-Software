<project>
    <masterversions>
        <package name="DirtySDK"                        version="8.17.0"/>    <!-- replaced by release script target-change-version.xml -->
        <package name="Framework"                       version="2.15.04"/>
        <package name="eaconfig"                        version="1.33.00"/>
        <package name="UnixGCC"                         version="0.02.00"/> 

    </masterversions>

    <buildroot>build</buildroot>
    <ondemand>true</ondemand>

    <globalproperties>
      dirtysdk-force-stripped
      dirtysdk-prebuilt-libs
      dirtysdk-samples
    </globalproperties>

    <config package="DirtySDK" default="unix64-gcc-dev-debug" includes="unix64-gcc-dev-*"/>

    <packageroots>
      <packageroot>../../</packageroot>
      <packageroot>../../../packages</packageroot>
      <packageroot>../../../../packages</packageroot>
      <packageroot>../../../../../packages</packageroot>
      <packageroot>${nant.location}/../../..</packageroot>

      <packageroot>../../packages/internal</packageroot>
      <packageroot>../../packages/custom-internal</packageroot>
      <packageroot>../../packages/custom-external</packageroot>
      <packageroot>../../packages/external</packageroot>
      <packageroot>../../packages/common</packageroot>

      <packageroot>../../../packages/internal</packageroot>
      <packageroot>../../../packages/custom-internal</packageroot>
      <packageroot>../../../packages/custom-external</packageroot>
      <packageroot>../../../packages/external</packageroot>
      <packageroot>../../../packages/common</packageroot>

      <packageroot>../../../../packages/internal</packageroot>
      <packageroot>../../../../packages/custom-internal</packageroot>
      <packageroot>../../../../packages/custom-external</packageroot>
      <packageroot>../../../../packages/external</packageroot>
      <packageroot>../../../../packages/common</packageroot>

      <packageroot>../../../../../packages/internal</packageroot>
      <packageroot>../../../../../packages/custom-internal</packageroot>
      <packageroot>../../../../../packages/custom-external</packageroot>
      <packageroot>../../../../../packages/external</packageroot>
      <packageroot>../../../../../packages/common</packageroot>
    </packageroots>
    
</project>
