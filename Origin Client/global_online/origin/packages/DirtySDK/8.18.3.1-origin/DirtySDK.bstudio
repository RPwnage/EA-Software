        <!--
        ================================================================================
        Build public Doxygen doc (static functions ignored)
        ================================================================================
        -->
        <BuildTask Label="BuildPublicDoc" UseSelectedPlatform="true" UseSelectedBuildType="true" Checked="false" StopOnFail="true">
            <BuildProgram>C:\packages\Framework\2.15.09\bin\nant.exe</BuildProgram>
            <CommandPrefix>dirtysdk-builddoc -D:config=${Platform}-dev-${BuildType} </CommandPrefix>
            <BuildOptions>
                <CheckBoxOption Label="Verbose" Checked="true">
                    <CheckedArguments>-verbose</CheckedArguments>
                </CheckBoxOption>
            </BuildOptions>
        </BuildTask>

        <!--
        ================================================================================
        Build full Doxygen doc (static functions included)
        ================================================================================
        -->
        <BuildTask Label="BuildFullDoc" UseSelectedPlatform="true" UseSelectedBuildType="true" Checked="false" StopOnFail="true">
            <BuildProgram>C:\packages\Framework\2.15.09\bin\nant.exe</BuildProgram>
            <CommandPrefix>dirtysdk-buildfulldoc -D:config=${Platform}-dev-${BuildType} </CommandPrefix>
            <BuildOptions>
                <CheckBoxOption Label="Verbose" Checked="true">
                    <CheckedArguments>-verbose</CheckedArguments>
                </CheckBoxOption>
            </BuildOptions>
        </BuildTask>

        <!--
        ================================================================================
        Perform a xenon deploy
        ================================================================================
        -->
        <BuildTask Label="XBox360 Deploy" UseSelectedPlatform="true" UseSelectedBuildType="true" Checked="false" StopOnFail="false">
            <BuildProgram>C:\packages\Framework\2.15.09\bin\nant.exe</BuildProgram>
            <CommandPrefix>runtime.atgframework.postbuildtarget runtime.T2Host.postbuildtarget -D:Deploy-Files=true -D:config=${Platform}-dev-${BuildType} </CommandPrefix>
            <BuildOptions>
                <CheckBoxOption Label="Verbose" Checked="true">
                    <CheckedArguments>-verbose</CheckedArguments>
                </CheckBoxOption>

                <TextBoxOption Label="Buildfile Location" ArgumentPrefix="-buildfile:">
                    <Argument>DirtySDK.build</Argument>
                </TextBoxOption>

                <TextBoxOption Label="Xenon Target List" ArgumentPrefix="-D:TargetXenon=">
                    <Argument> </Argument>
                </TextBoxOption>

                <TextBoxOption Label="Xenon Destination Directory" ArgumentPrefix="-D:Xenon-Dest-Dir=">
                    <Argument>DirtySDK</Argument>
                </TextBoxOption>
            </BuildOptions>
        </BuildTask>

        <RunTrigger Label="Perforce">
            <Program>C:\Program Files\Perforce\P4Win.exe</Program>
