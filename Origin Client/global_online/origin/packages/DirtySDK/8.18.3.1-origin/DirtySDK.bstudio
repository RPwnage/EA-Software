    <!-- List of Gui elements for build options -->
    <GuiElements>

        <!--
        ================================================================================
        Perform the Clean task
        ================================================================================
        -->
        <BuildTask Label="Clean" UncheckAfterBuild="true">
            <BuildProgram>C:\packages\Framework\2.15.09\bin\nant.exe</BuildProgram>
            <CommandPrefix>clean -D:config=${Platform}-dev-${BuildType} </CommandPrefix>
            <BuildOptions>
                <CheckBoxOption Label="Verbose" Checked="false">
                    <CheckedArguments>-verbose</CheckedArguments>
                </CheckBoxOption>

                <TextBoxOption Label="Buildfile Location" ArgumentPrefix="-buildfile:">
                    <Argument>DirtySDK.build</Argument>
                </TextBoxOption>

                <CheckBoxOption Label="Pass Config" Checked="false">
                    <CheckedArguments>${PlatformConfig} </CheckedArguments>
                </CheckBoxOption>
            </BuildOptions>
            <!-- This is used to pass the Platform Options, such as Version and Date
            <CommandSuffix>${PlatformConfig}</CommandSuffix>
            -->
        </BuildTask>

        <!--
        ================================================================================
        Create a VisualStudio vcproj file
        ================================================================================
        -->
        <BuildTask Label="nantvcproj" UseSelectedPlatform="true" UseSelectedBuildType="true">
            <BuildProgram>C:\packages\Framework\2.15.09\bin\nant.exe</BuildProgram>
            <CommandPrefix>-D:config=${Platform}-dev-${BuildType} </CommandPrefix>
            <CommandSuffix>${PlatformConfig} nantvcproj</CommandSuffix>
            <BuildOptions>
                <CheckBoxOption Label="Verbose" Checked="true">
                    <CheckedArguments>-verbose</CheckedArguments>
                </CheckBoxOption>

                <TextBoxOption Label="Buildfile Location" ArgumentPrefix="-buildfile:">
                    <Argument>DirtySDK.build</Argument>
                </TextBoxOption>

                <!--
                <TextBoxOption Label="Version" ArgumentPrefix="-D:dirtysdk-version=">
                    <Argument> </Argument>
                </TextBoxOption>

                <TextBoxOption Label="Date" ArgumentPrefix="-D:dirtysdk-date=">
                    <Argument> </Argument>
                </TextBoxOption>
                -->
            </BuildOptions>
        </BuildTask>

        <!--
        ================================================================================
        Perform a typical Build
        ================================================================================
        -->
        <BuildTask Label="Build" UseSelectedPlatform="true" UseSelectedBuildType="true">
            <BuildProgram>C:\packages\Framework\2.15.09\bin\nant.exe</BuildProgram>
            <CommandPrefix>-D:config=${Platform}-dev-${BuildType} </CommandPrefix>
            <CommandSuffix>${PlatformConfig}</CommandSuffix>
            <BuildOptions>
                <CheckBoxOption Label="Verbose" Checked="true">
                    <CheckedArguments>-verbose</CheckedArguments>
                </CheckBoxOption>

                <TextBoxOption Label="Buildfile Location" ArgumentPrefix="-buildfile:">
                    <Argument>DirtySDK.build</Argument>
                </TextBoxOption>

                <TextBoxOption Label="Version" ArgumentPrefix="-D:dirtysdk-version=">
                    <Argument> </Argument>
                </TextBoxOption>

                <TextBoxOption Label="Date" ArgumentPrefix="-D:dirtysdk-date=">
                    <Argument> </Argument>
                </TextBoxOption>

                <!--
                <CheckBoxOption Label="Build sln" Checked="true">
                    <CheckedArguments>-D:build-sln=true</CheckedArguments>
                    <UncheckedArguments>-D:build-sln=false</UncheckedArguments>
                </CheckBoxOption>
                -->

                <ComboBoxOption Label="Build Targets">
                    <Item Label="All" Default="true"></Item>
                    <Item Label="Code" Default="false">code</Item>
                    <Item Label="Data" Default="false">data</Item>
                    <!--
                    <Item Label="NHL2006 Only" Default="false">nhl2006-only</Item>
                    -->
                    <Item Label="DVD" Default="false">DVD</Item>
                </ComboBoxOption>

            </BuildOptions>
        </BuildTask>

