<?xml version="1.0"?>
<StudioDocument xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  
    <!-- List of build logs file -->
    <LastBuildLogFilename>BuildLog\LastBuild.log</LastBuildLogFilename>
    <AccumulatedBuildLogFilename>BuildLog\AccumulatedBuild.log</AccumulatedBuildLogFilename>

    <!-- List of platforms supported by the project -->
    <Platforms>
        <Platform>android-gcc</Platform>
        <Platform>pc-vc</Platform>
        <Platform>pc64-vc</Platform>
        <Platform>playbook-arm-gcc</Platform>
        <Platform>playbook-x86-gcc</Platform>
        <Platform>ps3-gcc</Platform>
        <Platform>ps3-sn</Platform>
        <Platform>psp2-sn</Platform>
        <Platform>rev-mw</Platform>
        <Platform>xenon-vc</Platform>
    </Platforms>

    <!-- List of build types supported by the project -->
    <BuildTypes>
        <BuildType>debug</BuildType>
        <BuildType>debug-opt</BuildType>
        <BuildType>opt</BuildType>
    </BuildTypes>

    <!-- List of filters for the log explorer -->
    <LogExplorer>
        <Filter>BuildLog\*.*</Filter>
    </LogExplorer>

    <!-- List of links to be displayed for quick access -->
    <HtmlLinks>
        <Link Label="DirtySock Home">http://online.ea.com/confluence/display/dirtysock/DirtySock+Customer+Portal</Link>
        <Link Label="BuildForge" Icon="Build Machine">http://tibbuildforge/index.php?Page=Builds</Link>
        <Link Label="DS BS Help">doc\api\BSHelp.html</Link>
    </HtmlLinks>

