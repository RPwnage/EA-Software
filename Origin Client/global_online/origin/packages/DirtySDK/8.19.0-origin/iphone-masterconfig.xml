<project>
    <masterversions>
        <package name="DirtySDK"                        version="8.19.0"/>
        <package name="Framework"                       version="2.15.14"/>
        <package name="eaconfig"                        version="1.34.02"/>
        <package name="osx_config"                      version="1.00.01"/>
        <package name="UnixGCC"                         version="0.04.00"/>
        <package name="iphonesdk"                       version="5.0-proxy"/>
    </masterversions>

    <buildroot>build</buildroot>
    <ondemand>true</ondemand>

    <globalproperties>
      dirtysdk-force-stripped
      dirtysdk-distpktwindow-size
      dirtysdk-prebuilt-libs
      dirtysdk-samples
      iphone-base-sdk-version=5.0
      iphone-extra-frameworks="CFNetwork Foundation"
      xcode-projectize
    </globalproperties>

    <config package="DirtySDK" default="iphone-gcc-dev-debug" includes="iphone-gcc-dev-* iphonesim-gcc-dev-* osx-gcc-dev*"/>

    <packageroots>
      <packageroot>../../../packages</packageroot>
      <packageroot>../../../../packages</packageroot>
      <packageroot>../../../../../packages</packageroot>
      <packageroot>../../</packageroot>
      <packageroot>${nant.location}/../../..</packageroot>

      <packageroot>../../packages/internal</packageroot>
      <packageroot>../../packages/custom-internal</packageroot>
      <packageroot>../../packages/custom-external</packageroot>
      <packageroot>../../packages/external</packageroot>
      <packageroot>../../packages/common</packageroot>

      <packageroot>../../../packages/internal</packageroot>
      <packageroot>../../../packages/custom-internal</packageroot>
      <packageroot>../../../packages/custom-external</packageroot>
      <packageroot>../../../packages/external</packageroot>
      <packageroot>../../../packages/common</packageroot>

      <packageroot>../../../../packages/internal</packageroot>
      <packageroot>../../../../packages/custom-internal</packageroot>
      <packageroot>../../../../packages/custom-external</packageroot>
      <packageroot>../../../../packages/external</packageroot>
      <packageroot>../../../../packages/common</packageroot>

      <packageroot>../../../../../packages/internal</packageroot>
      <packageroot>../../../../../packages/custom-internal</packageroot>
      <packageroot>../../../../../packages/custom-external</packageroot>
      <packageroot>../../../../../packages/external</packageroot>
      <packageroot>../../../../../packages/common</packageroot>
    </packageroots>
    
</project>
