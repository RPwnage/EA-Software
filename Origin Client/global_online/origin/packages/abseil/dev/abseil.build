  <property name="eaconfig.warningsaserrors" value="off"/>  



  <Library name="base" outputname="base${package.abseil.suffix}">
    <buildtype name="Library"/>
    <config>
      <defines>
        UNICODE
        WIN32
		_WINDOWS
        abseil_LIB
        <do if="${config-system} == pc64">
          WIN64
        </do>
      </defines>
      <warningsuppression>
		<do if="${config-compiler}==vc">
		  -wd4005
		  -wd4068
		  -wd4180
		  -wd4244
		  -wd4267
		  -wd4503
		  -wd4800
      	</do>
		</warningsuppression>
      <buildoptions>
        <option name="buildset.cc.options" if="${config-system} == pc">
          ${option.value}
          /Zc:wchar_t
        </option>
        <option name="buildset.cc.options" if="${config-system} == pc64 AND ${config-system} == pc">
        </option>
        <option name="buildset.lib.options" if="${config-system} == pc64">
          -machine:x64
        </option>
      </buildoptions>
      <remove>
        <defines>
          _CHAR16T
        </defines>
        <cc.options>
        </cc.options>
      </remove>
    </config>
    <dependencies>
      <use>
      </use>
      <build>
      </build>
    </dependencies>
    <headerfiles>
      <includes name="absl/base/**/*.h"/>
