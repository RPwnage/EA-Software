	  <includes name="absl/base/internal/thread_identity.cc"/>
	  <includes name="absl/base/internal/unscaledcycleclock.cc"/>
	  <includes name="absl/base/log_severity.cc"/>
    </sourcefiles>
    <buildsteps>
    </buildsteps>
    <includedirs>
	  ${package.dir}/
	  ${package.dir}absl/base/
	  ${package.dir}absl/base/internal/
    </includedirs>
    <resourcefiles>
    </resourcefiles>
    <visualstudio>
      <excludedbuildfiles>
      </excludedbuildfiles>
    </visualstudio>
  </Library>
    
