{
    "stateSpecific": {
        "wishlistTelemetryConfig": {
            "stateName": [
                "app.profile.topic",
                "app.profile.user.topic"
            ],
            "clickEvents" : {
                "PackArt" : {
                    "trigger": ".origin-telemetry-profile-wishlist-gametile .origin-telemetry-profile-wishlist-gametile-packart",
                    "telemetryEventData" : {
                        "category": "Wishlist",
                        "action" : {
                            "app.profile.topic": "PDP - Packart - Self",
                            "app.profile.user.topic": "PDP - Packart - Other"
                        },
                        "label" : "{offerId}"
                    }
                },
                "Title" : {
                    "trigger": ".origin-telemetry-profile-wishlist-gametile .origin-telemetry-profile-wishlist-gametile-title",
                    "telemetryEventData" : {
                        "category": "Wishlist",
                        "action" : {
                            "app.profile.topic": "PDP - Title - Self",
                            "app.profile.user.topic": "PDP - Title - Other"
                        },
                        "label" : "{offerId}"
                    }
                }
            },
            "mouseUpCustomEvent" : {
                "RemoveFromWishlist": {
                    "trigger": ".origin-telemetry-profile-wishlist-gametile .origin-telemetry-profile-wishlist-gametile-remove-link",
                    "telemetryEventData": {
                        "category": "Wishlist",
                        "action": "Remove",
                        "label": "{offerId}"
                    }
                },
                "OriginAccessCta" : {
                    "trigger": [
                        ".origin-telemetry-profile-wishlist-gametile .origin-telemetry-cta-primary .origin-telemetry-otkbtn-primary"
                    ],
                    "telemetryEventData": {
                        "category": "Wishlist",
                        "action" : {
                            "app.profile.topic": "Buy Now - Origin Access - Self",
                            "app.profile.user.topic": "Buy Now - Origin Access - Other"
                        },
                        "label": "{offerId}"
                    }
                },
                "PurchaseCta" : {
                    "trigger": [
                        ".origin-telemetry-profile-wishlist-gametile .origin-telemetry-store-purchase-cta",
                        ".origin-telemetry-profile-wishlist-gametile .origin-telemetry-gifting-cta"
                    ],
                    "telemetryEventData": {
                        "category": "Wishlist",
                        "action" : {
                            "app.profile.topic": "Buy Now - Self",
                            "app.profile.user.topic": "Buy Now - Gift"
                        },
                        "label": "{offerId}"
                    }
                },
                "ShareWishlistLink" : {
                    "trigger" :  [
                        ".origin-telemetry-share-wishlist-link"
                    ],
                    "telemetryEventData": {
                        "category": "Wishlist",
                        "action" : "Social",
                        "label": "Link"
                    }
                },
                "ShareWishlistFacebook" : {
                    "trigger" :  [
                        ".origin-telemetry-share-wishlist-facebook"
                    ],
                    "telemetryEventData": {
                        "category": "Wishlist",
                        "action" : "Social",
                        "label": "Facebook"
                    }
                }
            },
            "customEvents": "TelemetryWishlistCustomEventsFactory"
        }

    }
}