{
    "stateSpecific": {
        "storeHomePageTelemetryConfig": {
            "stateName": "app.store.wrapper.home",
            "category": "Store Home Page",
            "mouseUpEvents": {
                "Feature Carousel Cta": {
                    "trigger": [".origin-telemetry-store-carousel-featured-carousel .origin-telemetry-otkbtn-transparent"],
                    "action": "Click",
                    "label": "{title}",
                    "labelToken": {
                        "{title}": {
                            "root": ".origin-telemetry-store-carousel-featured-carousel",
                            "selector": "[title-str]"
                        }
                    }
                },
                "Feature Carousel Left Arrow": {
                    "trigger": [".origin-telemetry-store-carousel-featured-carousel .origin-telemetry-store-carousel-product-controls-left"],
                    "action": "Carousel",
                    "label": "{title}",
                    "labelToken": {
                        "{title}": {
                            "root": ".origin-telemetry-store-carousel-featured-carousel",
                            "selector": "[title-str]"
                        }
                    },
                    "value": "1"
                },
                "Feature Carousel Right Arrow": {
                    "trigger": [".origin-telemetry-store-carousel-featured-carousel .origin-telemetry-store-carousel-product-controls-right"],
                    "action": "Carousel",
                    "label": "{title}",
                    "labelToken": {
                        "{title}": {
                            "root": ".origin-telemetry-store-carousel-featured-carousel",
                            "selector": "[title-str]"
                        }
                    },
                    "value": "2"
                },
                "Feature Carousel Pill": {
                    "trigger": [".origin-telemetry-store-carousel-featured-carousel .origin-telemetry-store-carousel-product-indicator"],
                    "action": "Carousel",
                    "label": "{title}",
                    "labelToken": {
                        "{title}": {
                            "root": ".origin-telemetry-store-carousel-featured-carousel",
                            "selector": "[title-str]"
                        }
                    },
                    "value": "3"
                },
                "Game Tile": {
                    "trigger": [
                        ".origin-telemetry-storegametile .origin-telemetry-otkbtn-primary",
                        ".origin-telemetry-storegametile .origin-telemetry-otkbtn-primary-text",
                        ".origin-telemetry-storegametile .origin-telemetry-storegametile-overlay",
                        ".origin-telemetry-storegametile .origin-telemetry-storegametile-title-text"
                    ],
                    "action": "Product",
                    "label": "{offer-id}",
                    "labelToken": {
                        "{offer-id}": {
                            "root": ".origin-telemetry-storegametile",
                            "selector": ".origin-telemetry-storegametile-cta[offer-id]"
                        }
                    }
                },
                "Hero Carousel Left Arrow": {
                    "trigger": [".origin-telemetry-otkicon-leftarrowcircle"],
                    "action": "Carousel",
                    "label": "Heroic",
                    "value": "1"
                },
                "Hero Carousel Right Arrow": {
                    "trigger": [".origin-telemetry-otkicon-rightarrowcircle"],
                    "action": "Carousel",
                    "label": "Heroic",
                    "value": "2"
                },
                "Hero Carousel Pill": {
                    "trigger": [".origin-telemetry-store-carousel-hero-indicator"],
                    "action": "Carousel",
                    "label": "Heroic",
                    "value": "3"
                },
