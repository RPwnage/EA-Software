                        ".origin-telemetry-store-promo-feature .origin-telemetry-otkbtn-externallink"
                    ],
                    "action": "Click",
                    "label": "{id}",
                    "labelToken": {
                        "{id}": {
                            "priority": {
                                "image": {
                                    "root": ".origin-telemetry-store-promo-feature",
                                    "selector": "[image]"
                                },
                                "href": {
                                    "root": ".origin-telemetry-store-promo-feature",
                                    "selector": "[href]"
                                }
                            }
                        }
                    }
                },
                "Merchandised Tile": {
                    "trigger": [
                        ".origin-telemetry-store-promo .origin-telemetry-backgroundimage-image",
                        ".origin-telemetry-store-promo .origin-telemetry-backgroundimage-scrim",
                        ".origin-telemetry-store-promo .origin-telemetry-store-backgroundimagecarousel-image",
                        ".origin-telemetry-store-promo .origin-telemetry-store-promo-banner",
                        ".origin-telemetry-store-promo .origin-telemetry-store-promo-banner-content",
                        ".origin-telemetry-store-promo .origin-telemetry-store-promo-banner-title",
                        ".origin-telemetry-store-promo .origin-telemetry-store-promo-banner-subtitleText",
                        ".origin-telemetry-store-promo .origin-telemetry-store-promo-packart",
                        ".origin-telemetry-store-promo .origin-telemetry-price",
                        ".origin-telemetry-store-promo .origin-telemetry-strike-price",
                        ".origin-telemetry-store-promo .origin-telemetry-callout-text",
                        ".origin-telemetry-store-promo .origin-telemetry-otka"
                    ],
                    "action": "Click",
                    "label": "{id}",
                    "labelToken": {
                        "{id}": {
                            "priority": {
                                "ctid": {
                                    "root": ".origin-telemetry-store-promo",
                                    "selector": "[ctid]"
                                },
                                "ocd path": {
                                    "root": ".origin-telemetry-store-promo",
                                    "selector": "[ocd-path]"
                                },
                                "image path": {
                                    "root": ".origin-telemetry-store-promo",
                                    "selector": "[image]"
                                },
                                "title": {
                                    "root": ".origin-telemetry-store-promo",
                                    "selector": "[title-str]"
                                }
                            }
                        }
                    }
                },
                "Product Carousel Left Arrow": {
                    "trigger": [".origin-telemetry-store-carousel-product-carousel .origin-telemetry-store-carousel-product-controls-left"],
                    "action": "Carousel",
                    "label": "{title}",
                    "labelToken": {
                        "{title}": {
                            "root": ".origin-telemetry-store-carousel-product-carousel",
                            "selector": "[title-str]"
                        }
                    },
                    "value": "1"
                },
                "Product Carousel Right Arrow": {
                    "trigger": [".origin-telemetry-store-carousel-product-carousel .origin-telemetry-store-carousel-product-controls-right"],
                    "action": "Carousel",
                    "label": "{title}",
                    "labelToken": {
                        "{title}": {
                            "root": ".origin-telemetry-store-carousel-product-carousel",
                            "selector": "[title-str]"
                        }
                    },
                    "value": "2"
                },
                "Product Carousel Pill": {
                    "trigger": [".origin-telemetry-store-carousel-product-carousel .origin-telemetry-store-carousel-product-indicator"],
                    "action": "Carousel",
                    "label": "{title}",
                    "labelToken": {
                        "{title}": {
                            "root": ".origin-telemetry-store-carousel-product-carousel",
