{
    "stateSpecific": {
        "giftingTelemetryConfig": {
            "stateName": [
                "app.store.wrapper.pdp",
                "app.store.wrapper.addon",
                "app.game_gamelibrary.ogd.topic",
                "app.profile.user.topic"
            ],
            "pageViewEvent": {
                "telemetryGiftingStartFlow": {
                    "page": "/gifting/selectUser",
                    "params": {}
                },
                "telemetryGiftingMessagePage": {
                    "page": "/gifting/message",
                    "params": {}
                }
            },
            "telemetryGiftingNoSearchResults": {
                "category": "Gifting",
                "action": "Error",
                "label": "No Results Found{threshold}"
            },
            "mouseUpCustomEvent": {
                "SelectableFriendTile": {
                    "trigger": [
                        ".origin-telemetry-store-gift-friendtile"
                    ],
                    "category": "Gifting",
                    "action": "User Selection",
                    "label": {
                        "select": "Select",
                        "deselect": "Deselect",
                        "reselect": "Reselect"
                    }
                }
            },
            "blurEvent": {
                "Username": {
                    "trigger": [
                        ".origin-telemetry-store-gift-custommessage .origin-telemetry-store-gift-custommessage-yourname"
                    ],
                    "category": "Gifting",
                    "action": "Error",
                    "label": "Empty Sender"
                }
            },
            "customEvents": "TelemetryGiftingCustomEventsFactory"
        }
    }
}