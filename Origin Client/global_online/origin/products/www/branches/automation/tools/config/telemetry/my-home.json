{
    "stateSpecific": {
        "myHomeTelemetryConfig": {
            "stateName": "app.home_loggedin",
            "category": "My Home",
            "customEvents": "TelemetryMyHomeCustomEventsFactory",
            "scrollImpressionEvents": {
                "Friend Recommendation Tile" : {
                    "trigger": [
                        ".origin-telemetry-discovery-friendtile"
                    ],
                    "action": "Impression",
                    "label": "{id}",
                    "labelToken" : {
                        "{id}": {
                            "selector": "[ctid]"
                        }
                    },
                    "pinEvent" : {
                        "content" : {
                            "label" : "friend_recommendation",
                            "destination_name" : "my-home"
                        },
                        "meta" : {
                            "label" : "{friendId}",
                            "labelToken" : {
                                "{friendId}" : {
                                    "selector" : "[friendNucleusId]"
                                }
                            }
                        },
                        "reqs" : {
                            "label" : "{mutualFriendCounts}",
                            "labelToken" : {
                                "{mutualFriendCounts}" : {
                                    "selector" : "[mutualFriendCounts]"
                                }
                            }
                        }
                    }
                },
                "Recommended Next Action Tile": {
                    "trigger": [
                        ".origin-telemetry-home-recommended-tile"
                    ],
                    "action": "Impression",
                    "label": "{type}",
                    "labelToken": {
                        "{type}": {
                            "selector": "[telemetry-type]"
                        }
                    },
                    "pinEvent" : {
                        "content" : {
                            "label" : "recommended_next_action",
                            "destination_name" : "my-home"
                        }
                    }
                },
                "Discovery Tile and Get Started Tile": {
                    "trigger": [
                        ".origin-telemetry-discovery-tile"
                    ],
                    "action": "Impression",
                    "label": "{id}",
                    "labelToken": {
                        "{id}": {
                            "priority": {
                                "ctid": {
                                    "selector": "[ctid]"
                                },
                                "offerid": {
                                    "selector": "[offerid]"
                                },
                                "ocd path": {
                                    "selector": "[ocd-path]"
                                },
                                "image path": {
                                    "selector": "[image]"
                                },
                                "title": {
                                    "selector": "[title-str]"
                                },
                                "get started": {
                                    "selector": "[telemetry-name]"
                                }
                            }
                        }
                    },
                    "recommendation": {
                        "recommendationTrigger": ".origin-telemetry-discovery-tile",
                        "action": "impressions",
                        "label": "{recotype} {recotag} {recoid} {recoindex}",
                        "labelToken": {
                            "{recoid}": {
                                "selector": "[recoid]"
                            },
                            "{recoindex}": {
                                "selector": "[recoindex]"
                            },
                            "{recotype}": {
                                "selector": "[recotype]"
                            },
                            "{recotag}": {
                                "selector": "[recotag]"
                            }
                        }
                    },
                    "pinEvent": {
                        "content" : {
                            "label": "{header} : {body}",
                            "labelToken": {
                                "{header}": {
                                    "selector": "[descriptiontitle]"
                                },
                                "{body}": {
                                    "selector": "[description]"
                                }
                            },
                            "destination_name" : "my-home"
                        }
                    }
                }
            },
            "mouseUpEvents": {
                "Recommended Next Action Tile ID": {
                    "trigger": [
                        ".origin-telemetry-home-recommended-tile .origin-telemetry-otkbtn-primary",
                        ".origin-telemetry-home-recommended-tile .origin-telemetry-otkbtn-primary-text",
                        ".origin-telemetry-home-recommended-tile .origin-telemetry-tile-overlay",
                        ".origin-telemetry-home-recommended-tile .origin-telemetry-tile-overlay-content-title"
                    ],
                    "action": "RNA",
                    "label": "{id}",
                    "labelToken": {
                        "{id}": {
                            "priority": {
                                "ctid": {
                                    "root": ".origin-telemetry-home-recommended-tile",
                                    "selector": "[telemetry-ctid]"
                                },
                                "offer id": {
                                    "root": ".origin-telemetry-home-recommended-tile",
                                    "selector": "[telemetry-offerid]"
                                },
                                "image path": {
                                    "root": ".origin-telemetry-home-recommended-tile",
                                    "selector": "[telemetry-image]"
                                },
                                "text": {
                                    "root": ".origin-telemetry-home-recommended-tile",
                                    "selector": "[telemetry-subtitle]"
                                }
                            }
                        }
                    }
                },
                "Recommended Next Action Tile Action": {
                    "trigger": [
                        ".origin-telemetry-home-recommended-tile .origin-telemetry-otkbtn-primary",
                        ".origin-telemetry-home-recommended-tile .origin-telemetry-otkbtn-primary-text",
                        ".origin-telemetry-home-recommended-tile .origin-telemetry-tile-overlay",
                        ".origin-telemetry-home-recommended-tile .origin-telemetry-tile-overlay-content-title",
                        ".origin-telemetry-home-recommended-tile .origin-telemetry-otkicon-play",
                        ".origin-telemetry-home-recommended-tile .origin-telemetry-store-video"
                    ],
                    "action": "RNA",
                    "label": "{type}",
                    "labelToken": {
                        "{type}": {
                            "root": ".origin-telemetry-home-recommended-tile",
                            "selector": "[telemetry-type]"
                        }
                    }
                },
                "Discovery Tile": {
                    "trigger": [
                        ".origin-telemetry-discovery-tile .origin-telemetry-otkbtn-primary",
                        ".origin-telemetry-discovery-tile .origin-telemetry-otkbtn-primary-text",
                        ".origin-telemetry-discovery-tile .origin-telemetry-tile-overlay-content-title",
                        ".origin-telemetry-discovery-tile .origin-telemetry-otkicon-play",
                        ".origin-telemetry-discovery-tile .origin-telemetry-store-video"
                    ],
                    "action": "Click",
                    "label": "{id}",
                    "labelToken": {
                        "{id}": {
                            "priority": {
                                "ctid": {
                                    "root": ".origin-telemetry-discovery-tile",
                                    "selector": "[ctid]"
                                },
                                "offerid": {
                                    "root": ".origin-telemetry-discovery-tile",
                                    "selector": "[offerid]"
                                },
                                "ocd path": {
                                    "root": ".origin-telemetry-discovery-tile",
                                    "selector": "[ocd-path]"
                                },
                                "image path": {
                                    "root": ".origin-telemetry-discovery-tile",
                                    "selector": "[image]"
                                },
                                "title": {
                                    "root": ".origin-telemetry-discovery-tile",
                                    "selector": "[title-str]"
                                }
                            }
                        }
                    },
                    "recommendation": {
                        "recommendationTrigger": ".origin-telemetry-discovery-tile",
                        "action": "clicks",
                        "label": "{recotype} {recotag} {recoid} {recoindex}",
                        "labelToken": {
                            "{recoid}": {
                                "root": ".origin-telemetry-discovery-tile",
                                "selector": "[recoid]"
                            },
                            "{recoindex}": {
                                "root": ".origin-telemetry-discovery-tile",
                                "selector": "[recoindex]"
                            },
                            "{recotype}": {
                                "root": ".origin-telemetry-discovery-tile",
                                "selector": "[recotype]"
                            },
                            "{recotag}": {
                                "root": ".origin-telemetry-discovery-tile",
                                "selector": "[recotag]"
                            }
                        }
                    }
                },
                "Get Started Tile": {
                    "trigger": [
                        ".origin-telemetry-getstarted-tile .origin-telemetry-getstarted-btn-primary",
                        ".origin-telemetry-getstarted-tile .origin-telemetry-getstarted-learnmore",
                        ".origin-telemetry-getstarted-tile .origin-telemetry-getstarted-a"
                    ],
                    "action": "Click",
                    "label": "{id}",
                    "labelToken": {
                        "{id}": {
                            "root": ".origin-telemetry-getstarted-tile",
                            "selector": "[telemetry-name]"
                        }
                    }
                }
            }
        }
    }
}