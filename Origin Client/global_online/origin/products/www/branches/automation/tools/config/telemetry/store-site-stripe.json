{
    "global": {
        "storeSiteStripeTelemetryConfig": {
            "category": "Store Site Stripe",
            "mouseUpEvents": {
                "Store Site Stripe": {
                    "trigger": [".origin-telemetry-store-sitestripe-link"],
                    "action": "Click",
                    "label": "{id}",
                    "labelToken": {
                        "{id}": {
                            "selector": "[ctid]",
                            "root": ".origin-telemetry-store-sitestripe"
                        }
                    }
                }
            }, 
            "scrollImpressionEvents": {
                "Store Site Stripe": {
                    "trigger": [".origin-telemetry-store-sitestripe"],
                    "action": "Impression",
                    "label": "{id}",
                    "labelToken": {
                        "{id}": {
                            "selector": "[ctid]",
                            "root": ".origin-telemetry-store-sitestripe"
                        }
                    }
                }
            }
        }
    }
}