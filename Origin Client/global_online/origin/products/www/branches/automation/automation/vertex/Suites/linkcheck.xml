<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Origin Web Store LinkCheck" verbose="1" parallel="methods" thread-count="10" data-provider-thread-count="5">
    
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
        <listener class-name="com.ea.ng.common.listeners.MXMLReporter" />
        <listener class-name="com.ea.ng.common.listeners.MIInvokedMethodListener" />
        <listener class-name="com.ea.ng.common.listeners.MITestListener" />
    </listeners>
    
    <parameter name="hub" value="remote" />
    <parameter name="hubHost" value="eac-gavitron.eac.ad.ea.com" />
    <parameter name="hubPort" value="4444" />
    
    <parameter name="env" value="jenkins" />
    <parameter name="logLevel" value="2" />

    <parameter name="defaultDriverTimeout" value="10" />
    
    <parameter name="attempt_value" value="2" />
    
    <test name="LinkCheck">
        <classes>
            
        </classes>
    </test>
    
    <!-- Jenkins CI -->
    <parameter name="prod" value="https://store.dm.origin.com/" />
</suite>
