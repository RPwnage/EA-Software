<?xml version="1.0"?>
<enum>
	<description>User's chat state</description>

	<value name="ACTIVE">
		<description>User is actively participating in the chat session</description>
	</value>

	<value name="INACTIVE">
		<description>User has not been actively participating in the chat session</description>
	</value>

	<value name="GONE">
		<description>User has effectively ended their participation in the chat session</description>
	</value>

	<value name="COMPOSIING">
		<description>User is composing a message</description>
	</value>

	<value name="PAUSED">
		<description>USer has been composing but has now stopped</description>
	</value>
</enum>
