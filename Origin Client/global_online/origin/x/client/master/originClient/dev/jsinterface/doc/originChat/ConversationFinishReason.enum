<?xml version="1.0"?>
<enum>
	<description>Reason a conversation was finished</description>
	
	<value name="USER_FINISHED">
		<description>User voluntarily left the conversation</description>
	</value>
	
	<value name="FORCIBLY_FINISHED">
		<description>Conversation was forcibly finished for a reason out of the user's control</description>
	</value>
</enum>
