<?xml version="1.0"?>
<interface>
	<description>Represents an asynchronous query of users based on a pattern</description>
	<signal name="succeeded">
		<param name="searchInformation" type="SearchInfo[]" />
		<description>Emitted when the search REST api was successful.</description>
	</signal>
	
	<signal name="failed">
		<description>Emitted when the search REST api was unsuccessful.</description>
	</signal>
</interface>
