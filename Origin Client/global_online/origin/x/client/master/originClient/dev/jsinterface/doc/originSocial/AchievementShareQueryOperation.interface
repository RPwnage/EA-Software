<?xml version="1.0"?>
<interface>
	<description>Represents an asynchronous query of an user's privacy statuses including shared achievements.</description>
	<signal name="succeeded">
		<description>Emitted if the REST API query is succesful.</description>
		<param type="String" name="achievementShareInfo">
			<description>String containing a JSON structure with sharing statuses.</description>
		</param>
	</signal>
	<signal name="failed">
		<description>Emitted if the REST API query fails</description>
	</signal>
</interface>
