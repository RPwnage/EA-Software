<?xml version="1.0"?>
<interface root="true">
	<description>Object for querying information related to Origin Developer Mode</description>
	<property name="userId" type="Integer" readonly="true">
		<description>The user's numerical Nucleus ID.</description>
	</property>

    <property name="odtVersion" type="String" readonly="true">
        <description>
            Returns the client version.
        </description>
    </property>

    <property name="unownedIds" type="String[]" readonly="true">
        <description>
            Returns a list of all productIds that the user has an override for, but not an entitlement for.
        </description>
    </property>

    <method name="reloadConfig" return="Boolean">
        <description>
            Refreshes the configuration from the on-disk EACore.ini.  Returns true if successful.
        </description>
    </method>

    <method name="importFile" return="Boolean">
        <description>
            Replaces the in-memory configuration from disk.  Returns true if successful.
        </description>
    </method>

    <method name="exportFile" return="Boolean">
        <description>
            Saves the in-memory configuration to disk.  Returns true if successful.
        </description>
    </method>

    <method name="removeContent" return="Boolean">
        <param name="id" type="String" />
        <description>
            Removes all overrides with the given id from the in-memory configuration.  Returns true if successful.
        </description>
    </method>

    <method name="applyChanges">
        <description>
            Saves the in-memory configuration to disk, and refreshes the client's 'My Games' page - which will update product override settings.  If the user made changes to the settings that 
			require a client restart, then the user is presented with the option of automatically restarting the client now, or manually restarting it later.
        </description>
    </method>

    <method name="cloudConfiguration">
        <param name="id" type="String[]" />
        <description>
            Array of cloud debug information, including the offer's cloud configuration and the matching files on the user's drive if the title is installed.
        </description>
    </method>
    
    <method name="clearRemoteArea">
        <param name="id" type="String" />
        <description>
            Clears all cloud save content for the entitlement with the given id.
        </description>
    </method>

    <method name="selectFile" return="String">
        <param name="startingDirectory" type="String" />
        <param name="filter" type="String" />
        <param name="operation" type="String" />
        <description>
            Opens a file select dialog at startingDirectory.  Filter may be specified to accept a specific file extension.  Operation may be either "open" or "save".  Returns the resulting file path.
        </description>
    </method>

    <method name="readOverride" return="String">
        <param name="sectionName" type="String" />
        <param name="overrideName" type="String" />
        <description>
            Reads an override from the in-memory copy of the user's EACore.ini.  The sectionName parameter should be bracketed, i.e. "[QA]".
        </description>
    </method>

    <method name="readOverride" return="String">
        <param name="sectionName" type="String" />
        <param name="overrideName" type="String" />
        <param name="id" type="String" />
        <description>
            Reads a content-specific override from the in-memory copy of the user's EACore.ini.  The sectionName parameter should be bracketed, i.e. "[QA]".  The id parameter should be the productId of the entitlement.
        </description>
    </method>

    <method name="writeOverride" return="String">
        <param name="sectionName" type="String" />
        <param name="overrideName" type="String" />
        <param name="value" type="String" />
        <description>
            Writes an override to the in-memory copy of the user's EACore.ini.  The sectionName parameter should be bracketed, i.e. "[QA]".
        </description>
    </method>

    <method name="writeOverride" return="String">
        <param name="sectionName" type="String" />
        <param name="overrideName" type="String" />
        <param name="id" type="String" />
        <param name="value" type="String" />
        <description>
            Writes a content-specific override from the in-memory copy of the user's EACore.ini.  The sectionName parameter should be bracketed, i.e. "[QA]".  The id parameter should be the productId of the entitlement.
        </description>
    </method>

    <method name="openClientLog">
        <description>
            Opens the client log file in a text editor. Convenience functionality to allow ODT users to quickly access the client log.
        </description>
    </method>

    <method name="openBootstrapperLog">
        <description>
            Opens the bootstrapper log file in a text editor. Convenience functionality to allow ODT users to quickly access the bootstrapper log.
        </description>
    </method>
</interface>
