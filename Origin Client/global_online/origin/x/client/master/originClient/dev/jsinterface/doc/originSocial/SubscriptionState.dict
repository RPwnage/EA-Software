<?xml version="1.0"?>
<dict>
	<description>Description of the state of the presence subscription between a remote user and the current user</description>

	<member name="direction" type="SubscriptionDirection">
		<description>Direction of the subscription</description>
	</member>

	<member name="pendingContactApproval" type="Boolean">
		<description>Indicates if the subscription is pending approval from the remote user</description>
	</member>
	
	<member name="pendingCurrentUserApproval" type="Boolean">
		<description>Indicates if the subscription is pending approval from the current user</description> 
	</member>
</dict>
