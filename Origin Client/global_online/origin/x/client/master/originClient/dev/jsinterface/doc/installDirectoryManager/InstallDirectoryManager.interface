<?xml version="1.0"?>
<interface root="true">
	<description>Manages our game download directory and install cache.</description>
	<method name="chooseDownloadInPlaceLocation">
		<description>
			Opens a OS file dialog. Allows the user to change their game download directory.
		</description>
	</method>
	
	<method name="resetDownloadInPlaceLocation">
		<description>
			Resets the users game download directory.
		</description>
	</method>
	
	<method name="chooseInstallerCacheLocation">
		<description>
			Opens a OS file dialog. Allows the user to change their installer cache directory.
		</description>
	</method>
	
	<method name="resetInstallerCacheLocation">
		<description>
			Resets the users installer cache directory.
		</description>
	</method>
	
	<method name="browseInstallerCache">
		<description>
			Opens installer cache directory.
		</description>
	</method>
	
	<method name="deleteInstallers">
		<description>
			Deletes all installers in the installer cache directory.
		</description>
	</method>
	
</interface>
