<?xml version="1.0"?>
<interface root="true">
	<description>
		Root object for querying information about potentially unowned products
	</description>
	<method return="ProductQueryOperation" name="startQuery">
		<param name="productId" type="(String or String[])">
			<description>
				Single product ID to query or an array of product IDs. Querying additional product IDs has a low overhead compared to the initial query.
			</description>
		</param>
		<description>
			Asynchronously queries product information by product ID. A ProductQueryOperation object is returned which will signal completion once the query is complete.
		</description>
	</method>

	<method return="Boolean" name="userOwnsProductId">
		<param name="productId" type="String">
			<description>Product ID to check</description>
		</param>
		<description>
			Returns true if the current user owns the product associated with the passed product ID, false otherwise
		</description>
	</method>
	
	<method return="Boolean" name="userOwnsMultiplayerId">
		<param name="multiplayerId" type="String">
			<description>Multiplayer ID to check</description>
		</param>
		<description>
			Returns true if the current user owns the product compatible with the passed multiplayer ID, false otherwise
		</description>
	</method>
</interface>
