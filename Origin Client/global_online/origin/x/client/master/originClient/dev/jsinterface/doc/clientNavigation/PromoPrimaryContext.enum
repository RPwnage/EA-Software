<?xml version="1.0"?>
<enum>
	<description>
		Primary context enumeration for the promo manager.
	</description>

	<value name="OriginStarted">
		<description>After successful Origin log in.</description>	
	</value>
    
    <value name="GameFinished">
		<description>User exited a game session</description>	
	</value>
    
    <value name="FreeTrialExited">
		<description>User exited a free trial game session.</description>	
	</value>
    
    <value name="FreeTrialExpired">
		<description>The free trial game session has expired</description>	
	</value>
    
    <value name="DownloadUnderway">
		<description>The user started downloading something that's more than 500MByte.</description>	
	</value>
    
    <value name="MenuPromotions">
		<description>The user manually invoked the promo manager via Origin menu option.</description>	
	</value>
    
    <value name="NoContext">
		<description>No context specified.</description>	
	</value>
</enum>
