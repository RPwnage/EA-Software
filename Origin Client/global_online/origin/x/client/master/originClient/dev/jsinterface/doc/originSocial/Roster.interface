<?xml version="1.0"?>
<interface inherits="RosterView">
	<description>Representation of the user's complete roseter</description>

	<method name="removeContact">
		<param name="contact" type="RemoteUser" />
		<description>Prompts the user to remove the contact from their roster</description>
	</method>

	<property type="Boolean" name="hasLoaded" readonly="true">
		<description>Returns if the user's roster has been loaded from the server</description>
	</property>

	<signal name="loaded">
		<description>Emitted once the user's roster loads</description>
	</signal>
</interface>
