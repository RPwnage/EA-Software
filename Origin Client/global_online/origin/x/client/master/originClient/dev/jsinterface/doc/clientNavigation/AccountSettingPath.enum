<?xml version="1.0"?>
<enum>
	<description>
		Key to which account settings page should be shown.
	</description>

	<value name="Home">
		<description>Loads the main account settings page.</description>	
	</value>
    
    <value name="Privacy">
		<description>Loads the privacy settings page.</description>	
	</value>
    
    <value name="Security">
		<description>Loads the security settings page.</description>	
	</value>
    
    <value name="OrderHistory">
		<description>Loads the order history settings page.</description>	
	</value>
    
    <value name="PaymentShipping">
		<description>Loads the payment shipping settings page.</description>	
	</value>
    
    <value name="Redemption">
		<description>Loads the redemption settings page.</description>	
	</value>
</enum>
