<?xml version="1.0"?>
<interface>
	<description>
		Represents an asynchronous geo-country query operation
	</description>
	<signal name="succeeded">
		<param type="GeolocationInformation" name="info">
			<description>
				Resulting geo-country information.
			</description>
		</param>
		<description>
			Emitted if a geolocation query successfully completes
		</description>
	</signal>
	<signal name="failed">
		<description>
			Emitted if a geolocation query fails
		</description>
	</signal>
</interface>
