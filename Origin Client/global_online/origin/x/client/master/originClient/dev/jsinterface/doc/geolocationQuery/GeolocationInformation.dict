<?xml version="1.0"?>
<dict>
	<description>Geo-country information for an IP address</description>

	<member type="String" name="geoCountry">
		<description>
			The two-digit ISO code representing the geographical location of the IP address (i.e. "FR", "US", etc.)
		</description>
	</member>

    <member type="String" name="commerceCountry">
        <description>
			The two-digit ISO code representing the billing location of the IP address (i.e. "FR", "US", etc.).  May be different than geoCountry.
        </description>
    </member>

    <member type="String" name="commerceCurrency">
        <description>
            Description of the currency used by the country associated with the IP address (i.e. "EUR", "USD", etc.)
        </description>
    </member>
</dict>
