<?xml version="1.0"?>
<enum>
	<description>
		Display location; describes where the entitlement should appear in the interface
	</description>

	<value name="GAMES">
		<description>Should appear in the My Games page only.</description>
	</value>
	<value name="ADDONS">
		<description>Should appear in the addons table of the parent's Game Details page.</description>
	</value>
	<value name="EXPANSIONS">
		<description>Should appear in the expansions area of the parent's Game Details page.</description>
	</value>
    <value name="GAMES|EXPANSIONS">
        <description>Should appear in both the My Games page and the expansions area of the parent's Game Details page.</description>
    </value>
</enum>
