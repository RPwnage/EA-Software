<?xml version="1.0"?>
<enum>
	<description>
		Last known action of an install flow
	</description>

	<value name="NONE">
		<description>No install flow action was previously run.</description>	
	</value>

	<value name="DOWNLOAD">
		<description>The last install flow action was downloading an enitlement.</description>
	</value>
	
	<value name="PRELOAD">
		<description>The last install flow action was preloading an enitlement.</description>	
	</value>

	<value name="UPDATE">
		<description>The last install flow action was updating an enitlement.</description>
	</value>

	<value name="REPAIR">
		<description>The last install flow action was repairing an enitlement.</description>
	</value>
</enum>
