<?xml version="1.0"?>
<interface>
	<description>Represents an asynchronous query of an user's owned products</description>
  
	<signal name="succeeded">
		<param type="OwnedProductInformation[]" name="productInfo">
			<description>List of products owned by the user in arbitrary order</description>
		</param>
		<description>Emitted if an owned product query successfully completes</description>
	</signal>
  
	<signal name="failed">
		<description>Emitted if an owned product query fails</description>
	</signal>

  <method name="getUserProducts">
    <description>Starts a operation to get the user's products.  Should be called after callbacks are setup to avoid timing issues.</description>
  </method>
</interface>
