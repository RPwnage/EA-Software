<?xml version="1.0"?>
<enum>
    <description>
        Package types that determine how the downloader should handle the download of this game
    </description>

    <value name="DOWNLOAD_IN_PLACE">
        <description>Download In Place</description>
    </value>

    <value name="SINGLE">
        <description>A single file to be downloaded directly to the user's hard drive</description>
    </value>

    <value name="UNPACKED">
        <description>A zip/viv file to be downloaded and unzipped to the user's hard drive</description>
    </value>

    <value name="EXTERNAL_URL">
        <description>A browser game.  This type of game does not have a file download, but opens a configured URL in the user's default browser.</description>
    </value>

    <value name="UKNOWN">
        <description>Unrecognized or misconfigured package type</description>
    </value>
</enum>
