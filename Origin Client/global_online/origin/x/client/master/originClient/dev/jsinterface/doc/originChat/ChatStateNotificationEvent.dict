<?xml version="1.0"?>
<dict inherits="ConversationEvent">
	<description>XEP-0085 chat state notification</description>

	<member name="state" type="ChatState">
		<description>Current chat state</description>
	</member>
	
	<member name="from" type="SocialUser">
		<description>Sender of the chat state notification</description>
	</member>

	<member name="threadId" type="String">
		<description>Thread ID this chat state pertains to</description>
	</member>
</dict>
