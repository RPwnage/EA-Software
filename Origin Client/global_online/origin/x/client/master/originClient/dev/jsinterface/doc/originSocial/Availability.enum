<?xml version="1.0"?>
<enum>
	<description>Describes the advertised availability of a user</description>

	<value name="ONLINE">
		<description>User is online and available</description>
	</value>
	<value name="CHAT">
		<description>User is actively intereted in chatting</description>
	</value>
	<value name="AWAY">
		<description>User is temporarily away</description>
	</value>
	<value name="XA">
		<description>User is away for an extended periody</description>
	</value>
	<value name="DND">
		<description>User is busy</description>
	</value>
	<value name="UNAVAILABLE">
		<description>User is offline or invisible</description>
	</value>
</enum>
