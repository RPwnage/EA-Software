<?xml version="1.0"?>
<enum>
	<description>
		Secondary context enumeration for the promo manager.
	</description>

	<value name="GameTileContextMenu">
		<description>Invoked from the game-tile context menu.</description>	
	</value>
    
    <value name="GameHovercard">
		<description>Invoked from the game hovercard (within "My Games").</description>	
	</value>
    
    <value name="OwnedGameDetails">
		<description>Invoked from the game's "Owned Game Details" (OGD) page.</description>	
	</value>
    
    <value name="NoContext">
		<description>No context specified.</description>	
	</value>
</enum>
