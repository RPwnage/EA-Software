<?xml version="1.0"?>
<interface>
	<description>
		Represents an asynchronous On the House promo query operation
	</description>
	<signal name="succeeded">
		<param type="OnTheHousePromo" name="info">
			<description>
				Resulting On the House promotion information.
			</description>
		</param>
		<description>
			Emitted if an onTheHouse query successfully completes
		</description>
	</signal>
	<signal name="failed">
		<description>
			Emitted if an onTheHouse query fails
		</description>
	</signal>
</interface>
