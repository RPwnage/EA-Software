<?xml version="1.0"?>
<interface root="true">
	<description>
		Root object for querying On the House information
	</description>
	<method name="startQuery">
		<description>
			Queries current On the House promotion information. A succeeded signal is sent once the query is complete.
		</description>
	</method>
	<signal name="succeeded">
		<param type="OnTheHousePromo" name="info">
			<description>
				Resulting On the House promotion information.
			</description>
		</param>
		<description>
			Emitted if an onTheHouse query successfully completes
		</description>
	</signal>
	<signal name="failed">
		<description>
			Emitted if an onTheHouse query fails
		</description>
	</signal>
</interface>
