<?xml version="1.0"?>
<interface>
  <description>
    This object is used as a return value for the Achievement Statistics object. It contains an achievementSet, and an achievement object.
  </description>

  <property name="achievementSet" type="AchievementSet">
    <description>
      The achievement set of the achievement.
    </description>
  </property>

  <property name="achievement" type="Achievement">
    <description>
      The achievement.
    </description>
  </property>

</interface>
