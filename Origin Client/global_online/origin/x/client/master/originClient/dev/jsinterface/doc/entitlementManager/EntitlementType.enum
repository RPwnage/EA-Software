<?xml version="1.0"?>
<enum>
	<description>
		Entitlement type; distinguishes base games from additional content
	</description>

	<value name="GAME">
		<description>Standalone game.</description>
	</value>
    <value name="EXPANSION">
        <description>Expansions are large-scale, downloadable bonus content for a standalone game.</description>
    </value>
	<value name="PDLC">
		<description>PDLC entitlements are small-scale, downloadable bonus content for a standalone game.</description>
	</value>
	<value name="PULC">
        <description>PULC is downloaded as part of the base game and can be unlocked by a separate purchase.</description>
	</value>
    <value name="AUDIO">
        <description>An audio file.  Deprecated?</description>
    </value>
    <value name="VIDEO">
        <description>A video file.  Deprecated?</description>
    </value>
    <value name="PATCH">
        <description>A patch.  Deprecated?</description>
    </value>
    <value name="DEMO">
        <description>A demo.  Deprecated?</description>
    </value>
</enum>
