<?xml version="1.0"?>
<dict>
	<description>Information about a user's game being played through Origin</description>
	<member name="productId" type="String">
		<description>Product ID of the game the contact is playing.</description>
	</member>
	<member name="gameTitle" type="String">
		<description>Human readable title of the game the contact is playing in plain text in the playing user's locale. This value is remotely provided and must not be trusted. In particular, it may contain malicious HTML special characters.</description>
	</member>
	<member name="joinable" type="Boolean">
		<description>Indicates if the game has a joinable multiplayer session</description>
	</member>
	<member name="broadcastUrl" type="String?">
		<description>URL of the video broadcast for the game session or null if none exists</description>
	</member>
	<member name="purchasable" type="Boolean">
		<description>Indicates if the game available to purchase in the Store. Non-Origin games are not purchasable.</description>
	</member>
</dict>
