<?xml version="1.0"?>
<dict inherits="ConversationEvent">
	<description>Participant that has entered the conversation</description>
	
	<member name="user" type="SocialUser">
		<description>User entering the conversation</description>
	</member>

	<member name="nickname" type="String">
		<description>Nickname of the user in the context of the conversation. In mutli-user chats this nickname is specific to the multi-user chat room.</description>
	</member>
</dict>
