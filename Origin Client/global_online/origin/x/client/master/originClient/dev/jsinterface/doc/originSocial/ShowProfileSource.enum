<?xml version="1.0"?>
<enum>
	<description>Describes where the "showProfile" request came from</description>
	
	<value name="FRIENDS_LIST">
		<description>The request came from the friends list</description>
	</value>
	
	<value name="MY_PROFILE">
		<description>The request came from current user's profile</description>
	</value>
	
	<value name="FRIENDS_PROFILE">
		<description>The request came from friend's profile</description>
	</value>
	
	<value name="GAME_DETAILS">
		<description>The request came from game details</description>
	</value>
	
	<value name="CHAT">
		<description>The request came from a chat dialogue</description>
	</value>
	
	<value name="ACHIEVEMENTS">
		<description>The request came from the achievements page</description>
	</value>
	
	<value name="MYGAMES">
		<description>The request came from my games tab</description>
	</value>
	
	<value name="MAIN_CLIENT_WINDOW">
		<description>The request came from the main client window</description>
	</value>

	<value name="NON_FRIENDS_PROFILE">
		<description>The request came from a user's profile that is not a friend</description>
	</value>
</enum>
