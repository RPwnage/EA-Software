<?xml version="1.0"?>
<dict inherits="ConversationEvent">
	<description>Change of subscription state between a conversation  participant and the current user</description>

	<member name="user" type="SocialUser">
		<description>User changing subscription state with the current user</description>
	</member>

	<member name="subscriptionState" type="SubscriptionState">
		<description>New subscription state with the current user</description>
	</member>
    
    <member name="previousSubscriptionState" type="SubscriptionState">
		<description>Previous subscription state with the current user</description>
	</member>
</dict>
