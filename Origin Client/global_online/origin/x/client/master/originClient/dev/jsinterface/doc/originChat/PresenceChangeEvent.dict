<?xml version="1.0"?>
<dict inherits="ConversationEvent">
	<description>Presence change of a conversation participant</description>

	<member name="user" type="SocialUser">
		<description>User changing presence</description>
	</member>

	<member name="availability" type="Availability?">
		<description>New availability for the user or null if their availability is not known</description>
	</member>
	
	<member name="statusText" type="String?">
		<description>New status string for the user or null if it's unknown or unset</description>
	</member>

	<member name="playingGame" type="PlayingGame?">
		<description>Game being played by the user or null if no game is being played</description>
	</member>

	<member name="previousAvailability" type="Availability?">
		<description>Previous availability for the user or null if their availability was not known</description>
	</member>
	
	<member name="previousStatusText" type="String?">
		<description>New status string for the user or null if it was unknown or unset</description>
	</member>

	<member name="previousPlayingGame" type="PlayingGame?">
		<description>Game previously being played by the user or null if no game was being played</description>
	</member>
</dict>
