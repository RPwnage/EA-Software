<?xml version="1.0"?>
<enum>
	<description>Direction of a presence subscription</description>

	<value name="NONE">
		<description>No presence subscription exists</description>
	</value>

	<value name="TO">
		<description>The user is subscribed to the contact's presence; the contact is not subscribed to the user's presence</description>
	</value>

	<value name="FROM">
		<description>The contact is subscribed to the user's presence; the user is not subscribed to the contact's presence</description>
	</value>

	<value name="BOTH">
		<description>The user and contact are mutually subscribed to each other's presence</description>
	</value>
</enum>
