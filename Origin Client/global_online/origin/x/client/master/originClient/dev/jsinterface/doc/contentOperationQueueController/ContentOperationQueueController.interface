<?xml version="1.0"?>
<interface root="true">
	<description>Top level container for client settings</description>
	<property name="supportedlanguages" type="String[]" readonly="true">
		<description>List of locale codes that the Origin Client currently supports.</description>
	</property>
    
    <property name="areServerUserSettingsLoaded" type="Boolean" readonly="true">
		<description>Returns true if the user server-side settings have finished loading.</description>
	</property>
	
	<method name="restoreDefaultHotkey">
        <param name="hotkeySettingName" type="String" />
        <param name="hotkeyStrSettingName" type="String" />
		<description>
			Restores a hotkey value and strings to their default.
		</description>
	</method>
	
	<method name="writeSetting">
		<param name="settingName" type="String" />
		<param name="value" type="Object" />
		<description>
			Overwrites a setting contained in the SettingsManager.
		</description>
	</method>
	
	<method return="Object" name="readSetting">
		<param name="settingName" type="String" />
		<description>
			Returns current value of named setting.
		</description>
	</method>
	
	<method return="Object" name="settingIsDefault">
		<param name="settingName" type="String" />
		<description>
			Returns if the setting value is the default.
		</description>
	</method>
	
	<method name="stopLocalHost">
		<description>
			Stops the Origin Web Integration (local host).
		</description>
	</method>

	<method name="startLocalHost">
		<description>
			Start the Origin Web Integration (local host).
		</description>
	</method>
	
	
	<signal name="updateSettings">
		<description>
			Emitted when a setting from Setting Manager is changed.
		</description>
	</signal>
</interface>
