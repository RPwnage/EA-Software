<?xml version="1.0"?>
<interface root="true">
	<description>Provides high-level methods in-client navigation</description>

	<method name="showStoreHome">
		<description>Switches to the store tab and shows the home page of the store</description>
	</method>
	
	<method name="showStoreProductPage">
		<param name="productId" type="String">
			<description>
				Product ID of the product to show. This is exposed as Entitlement.productId
			</description>
		</param>
		<description>Switches to the store tab and shows the product page for the passed product ID</description>
	</method>
	
	<method name="showStoreMasterTitlePage">
		<param name="masterTitleId" type="String">
			<description>
				Master Title ID of the product to show. This is exposed as Entitlement.masterTitleId
			</description>
		</param>
		<description>Switches to the store tab and shows the store page for the passed master title ID.  Note that most base games and DLC share a master title ID so this will typically resolve to the base game's PDP.</description>
	</method>
	
	<method name="showStoreUrl">
		<param name="url" type="String">
			<description>
				Absolute URL to show on the store tab. The variables {site} and {locale} will be replaced with the calculated site and locale for the current client language.
			</description>
		</param>
		<description>Switches to the store tab and shows an arbitrary URL.</description>
	</method>
    
	<method name="showPromoManager">
		<param name="productId" type="String">
			<description>
				The product/offer ID which the promo manager should query promotions for.
			</description>
		</param>
        <param name="primaryContext" type="PromoPrimaryContext">
			<description>
				The "primary" context which the promo manager should be invoked under. See PromoBrowserContext::Primary (C++ client).
			</description>
		</param>
        <param name="secondaryContext" type="PromoSecondaryContext">
			<description>
				The "secondary" context which the promo manager should be invoked under. See PromoBrowserContext::Secondary (C++ client).
			</description>
		</param>
		<description>Invokes the promo manager for the given offer ID under the given primary and secondary contexts.</description>
	</method>

	<method name="showStoreOrderHistory">
		<description>Switches to the store tab and shows the user's order history</description>
	</method>

	<method name="launchExternalBrowser">
		<param name="url" type="String" />
		<description>Shows the passed URL in the user's default browser. This URL must be globally accessible; widget:// URLs will not be reachable to an external browser</description>
	</method>

	<method name="showPDLCStore">
		<param name="contentId" type="String">
			<description>
				Generic ID of the product to show. This is exposed as Entitlement.contentId, Entitlement.productId, or Entitlement.masterTitleId
			</description>
		</param>

		<description>Shows the PDLC store for the passed generic ID. This allows the user to buy expansions, addons, unlockable content, etc. If the IGO is active, the PDLC store will appear in the IGO.</description>
	</method>

	<method name="showDownloadProgressDialog">
		<description>Show a window containing detailed progress information for all in-progress downloads</description>
	</method>

	<method name="showGameDetails">
		<param name="contentId" type="String">
			<description>
				Content ID of the game details page to show. This is exposed as Entitlement.contentId
			</description>
		</param>
		<description>
			Shows in-client detailed information for a content ID owned by the current user. This method does nothing if the user does not own the passed content ID.
		</description>
	</method>
	
	<method name="showMyGames">
		<description>Switches to the my games tab and shows the users entitlements</description>
	</method>
	
	<method name="showMyProfile">
		<description>Switches to the current users profile</description>
	</method>
	
	<method name="showFriends">
		<description>Shows the users friends (Note: This is not functional due to current changes to the friends list)</description>
	</method>

	<method name="showStoreFreeGames">
		<description>Switches to the store tab and shows the free games landing page</description>
	</method>
	
	<method name="showStoreOnTheHouse">
		<param name="hasGames" type="Boolean">
			<description>
				Helps to determine if the user had games when they navigated to the On The House landing page.
			</description>
		</param>
		<description>
			Switches to the store tab and shows the On The House landing page
		</description>
	</method>

	<method name="showAvatarChooser">
		<description>Starts a native flow to allow the user to select a new avatar</description>
	</method>

    <method name="showAchievements">
        <description>Shows the current user's achievements</description>
    </method>
	
	<method name="showAchievementSetDetails">
		<param name="achievementSetId" type="String">
			<description>
				The identifier for the achievement set. See AchievementSet for more information
			</description>
		</param>
		<param name="userId" type="String">
			<description>
				The ID of the user whose achievement set's details will be displayed inside the client.
			</description>
		</param>
		<param name="gameTitle" type="String">
			<description>
				The title of the game which the achievement set's details is connected to.
			</description>
		</param>
        <description>Shows an achievement set's details based on the parameters passed</description>
    </method>

	<method name="showFriendSearchDialog">
		<description>Shows a dialog allowing users to search for friends or import friends via social networks</description>
	</method>

  <method name="showUrlInMyGames">
    <param name="url" type="String">
      <description>
        Absolute URL to show in the My Games tab.
      </description>
    </param>
      <description>
        Open the given URL in the My Games tab.
      </description>
   </method>

   <method name="showOriginHelp">
	   <description>Shows the Origin help page. It is not possible to deep link to content inside the Origin help system.</description>
   </method>
   
   	<method name="showMyAccount">
		<description>Requests the user's profile to be shown in the main window</description>
	</method>
    
    <method name="showMyAccountPath">
		<param name="path" type="AccountSettingPath">
		 <description>
		  Key to which account settings page should be shown.
		 </description>
		</param>
		<description>Requests the user's profile to be shown in the main window</description>
	</method>

   <method name="showProfileSearchResult">
	   <param name="keywords" type="String">
		   <description>Space separated list of keywords to search for</description>
	   </param>
	   <description>Shows a search result page listing profiles matching the passed keywords</description>
   </method>
</interface>
