<?xml version="1.0"?>
<interface root="false">
    <description>
        The achievement set release info object. This gives provide information about the released achievement sets. 
    </description>

  <!-- PROPERTIES -->
  <property type="String" name="achievementSetId" readonly="true">
    <description>
      The achievementSetId associated with this release info.
    </description>
  </property>
  <property type="String" name="masterTitleId" readonly="true">
    <description>
      The masterTitle of the game associated with this release info.
    </description>
  </property>
  <property type="String" name="displayName" readonly="true">
    <description>
      The game display name associated with this release info.
    </description>
  </property>
  <property type="String" name="platform" readonly="true">
    <description>
      A comma separated list of platforms supported by this achievement set.
    </description>
  </property>
 </interface>
