<?xml version="1.0"?>
<interface>
	<description>Represents a record of the time range and events of a conversation</description>
	
	<property name="events" type="ConversationEvent[]" readonly="true">
		<description>List of all events in this conversation in chronological order</description>
	</property>

	<property name="startTime" type="Date" readonly="true">
		<description>Start time for the conversation</description>
	</property>
	
	<property name="endTime" type="Date?" readonly="true">
		<description>End time for the conversation or null if the conversation is ongoing</description>
	</property>
</interface>
