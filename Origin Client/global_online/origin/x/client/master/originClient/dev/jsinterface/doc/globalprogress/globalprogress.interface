<?xml version="1.0"?>
<interface root="true">
	<description>Object describing the set of entitlements in the global progress list</description>

	<signal name="added">
		<description>Fired whenever an entitlement is added to the global progress list</description>
	</signal>
    
	<signal name="removed">
		<description>Fired whenever an entitlement is removed from the global progress list</description>
	</signal>    

	<property type="Entitlement[]" name="entitlements">
		<description>List of entitlements currently in the global progress list</description>
	</property>

	<method name="removeFromGlobalProgress">
		<description>removes an entitlement from the global progress list</description>
	</method>
</interface>
