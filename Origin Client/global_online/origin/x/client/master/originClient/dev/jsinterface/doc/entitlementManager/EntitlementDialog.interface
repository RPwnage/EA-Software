<?xml version="1.0"?>
<interface>
	<description>
		A list of dialogs associated with an entitlement.
	</description>

    <method name="showProperties">
      <param name="offerId" type="String" />
      <description>
        Shows a dialog with the entitlement's properties.
      </description>
    </method>
    
    <method name="showProperties">
      <param name="offerId" type="String" />
      <description>
        Shows a dialog that allows the user to modify the entitlement's box art if it's a Non-Origin game.
      </description>
    </method>
    
	<method name="promptForParentInstallation">
      <description>
        Prompts the user to install the parent entitlement as a dependency of this entitlement
      </description>
	</method>

	<method name="promptForDLCIncompatibleWithGame">
      <description>
        Prompts the user that the DLC they are trying to buy/download is incompatible with the base game and they should reinstall.
      </description>  
	</method>

	<method name="showDownloadDebug">
      <description>
        Show download debug information on a specific entitlement.
      </description>  
	</method>
</interface>
