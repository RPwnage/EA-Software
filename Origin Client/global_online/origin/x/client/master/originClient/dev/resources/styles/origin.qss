}

QMenu::item:enabled:selected {
    background-color: rgba(195, 198, 206, 0.5);
}

QMenu::separator{
    height: 1px;
    background: #fff;
	border-bottom: 1px solid rgba(195, 198, 206, 0.5);
}

QMenu::right-arrow {
	margin-left: 8px;
}

