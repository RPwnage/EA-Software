/************************************************************/
/* APP MAIN MENUS */
/************************************************************/

QMenuBar#mainMenuBar {
    background-color: transparent;
	color: #C3C6CE;
	font-size: 12px;
    font-weight: 600;
	min-height: 33px;
	max-height: 33px;
	cursor: pointer;
}

QMenuBar#mainMenuBar::item {
	cursor: pointer;
	margin-right: 4px;
	padding-left: 8px;
	padding-right: 8px;
    background: transparent;
	cursor: pointer;
}

QMenuBar#mainMenuBar::item:selected {
    color: #FFF;
	background: #22313c;
}

QMenuBar#mainMenuBar QMenu {
    background: #22313c;
	border: 0px solid rgba(120, 125, 133, 0.2);
	box-shadow: 0px 0px 15px 0px rgba(30, 38, 44, 0.5);
}

QMenuBar#mainMenuBar QMenu::item {
	color: #C3C6CE;
    font-size: 12px;
	font-weight: 400;
    margin: 0;
	padding: 6px 9px;
	min-width: 155px;
}

QMenuBar#mainMenuBar QMenu::item:selected {
    color: #FFF;
	background: #000;
}

QMenuBar#mainMenuBar QMenu::item:disabled {
    color: rgba(195, 198, 206, 0.3);
    border-color: transparent;
    background: transparent;
}

QMenuBar#mainMenuBar QMenu::item:checked {
	padding-left: 30px;
}

QMenuBar#mainMenuBar QMenu::separator {
	background: rgba(195, 198, 206, 0.3);
	border: none;
}

QMenuBar#mainMenuBar  QMenu::indicator {
    width: 12px;
    height: 10px;
}

/* non-exclusive indicator = check box style indicator (see QActionGroup::setExclusive) */
QMenuBar#mainMenuBar QMenu::indicator:non-exclusive:checked {
    image: url(:/platform/menu_checkmark.png);
	margin-left: 9px;
}

QMenuBar#mainMenuBar QMenu::right-arrow {
    image: url(:/platform/menu_rightarrow.png);
	margin-right: 12px;
}

QMenuBar#mainMenuBar QMenu::right-arrow:disabled {
    image: url(:/platform/menu_rightarrowdisabled.png);
	margin-right: 12px;
}

QMenuBar#mainMenuBar QMenu#mainSetStatusMenu::item,
QMenu#mainSetStatusMenu::item {
	padding-left: 21px;
}

QMenu#mainSetStatusMenu::item:checked,
QMenu#mainSetStatusMenu::item:checked:selected {
    background-image: url(:/platform/otkcheck.png);
    background-repeat: no-repeat;
    background-position: right center;
}

