<?xml version="1.0"?>
<enum>
	<description>Describes the visibility of the connected user</description>
	
	<value name="INVISIBLE">
		<description>User appears unavailable all other users</description>
	</value>
	
	<value name="VISIBLE">
		<description>User's actual presence is visible to all other users</description>
	</value>
</enum>
