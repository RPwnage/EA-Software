<?xml version="1.0"?>
<interface root="true">
	<description>Root object of Origin social support</description>

	<method name="createPlayingRosterView" return="RosterView">
		<param name="productIds" type="(String or String[])?">
			<description>Game product ID or IDs to filter on or null for all contacts currently playing a game</description>
		</param>

		<description>Creates a dynamic roster view of all contacts playing a game</description>
	</method>
	
	<method name="createOwningRosterView" return="RosterView">
		<param name="productIds" type="(String or String[])?">
			<description>Product ID or IDs to filter on</description>
		</param>

		<description>Creates a dynamic roster view of all contacts owning a specific or closely related game</description>
	</method>

	<method name="getUserById" return="SocialUser?">
		<param name="id" type="String" />
		<description>Returns the social user with the passed ID or null if no such user exists</description>
	</method>
	
	<method name="getRosterContactByPid" return="RemoteUser?">
		<param name="pid" type="String" />
		<description>Returns the roster contact with the passed PID (Nucleus user Id) or null if no such contact exists</description>
	</method>

  <method name="searchFriends" return="SearchFriendsOperation">
    <param name="searchKeyword" type="String" />
    <param name="pageNumber" type="String">
	    <description>
	    The page number you want to display. Each page is 20 lines. If no page number is passed, it will return page number 1
	    </description>
	</param>
    <description>Returns a set of records matching the passed searchkeyword, at the page passed as parameter. It also returns the total count of records this search contains. </description>
  </method>  

  <property name="roster" type="Roster" readonly="true">
		<description>Complete roster of the current user</description>
	</property>

  <property name="currentUser" type="ConnectedUser" readonly="true">
		<description>Currently connected XMPP user</description>
	</property>

	<property name="connection" type="Connection" readonly="true">
		<description>Information about the current XMPP connection</description> 
	</property>

	<property name="mutualContacts" type="RosterView">
		<description>Roster view of all contacts with which we have a mutual presence subscription</description> 
	</property>
	
	<method name="startMultiUserConversation">
		<param name="initialInvites" type="RemoteUser[]">
			<description>List of remote users to initially invite to the conversation</description>
		</param>
		<description>Starts a conversation with multiple users. This will typically open a new chat window or show an existing one.</description>
	</method>

	<method name="showCreateMultiUserConversationDialog">
		<param name="initialSelection" type="RemoteUser[]" optional="true">
			<description>List of remote users to initially select in the dialog</description>
		</param>

		<description>Shows a dialog allowing the user to select contacts to start a new multi-user conversation with</description>
	</method>
</interface>
