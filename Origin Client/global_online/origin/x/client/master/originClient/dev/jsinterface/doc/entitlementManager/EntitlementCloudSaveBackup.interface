<?xml version="1.0"?>
<interface>
	<description>Represents a backed up version of an entitlement's local save files</description>

	<property name="created" type="Date" readonly="true">
		<description>Date the backup was performed</description>
	</property>

	<method name="restore">
		<description>
			Starts an interactive flow to restore the entitlement's local save files from this backup. The flow will confirm the restore operation with the user before proceeding
		</description>
	</method>
</interface>
