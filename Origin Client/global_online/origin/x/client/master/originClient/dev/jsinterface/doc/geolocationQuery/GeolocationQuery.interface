<?xml version="1.0"?>
<interface root="true">
	<description>
		Root object for querying geo-country information for a given IP address
	</description>
	<method return="GeolocationQueryOperation" name="startQuery">
		<param name="ipAddress" type="String">
			<description>
				String representation of an IP address (i.e. 127.0.0.1)
			</description>
		</param>
		<description>
			Asynchronously queries geo-country information for the given IP address. A GeolocationQueryOperation object is returned which will signal completion once the query is complete.
		</description>
	</method>
</interface>
