<?xml version="1.0"?>
<interface>
	<description>Represents an asynchronous query of a user's contacts</description>
  
	<signal name="succeeded">
		<param type="SocialUser[]" name="contacts">
			<description>List of contacts of the user in arbitrary order</description>
		</param>
		<description>Emitted if an user contacts query successfully completes</description>
	</signal>
  
	<signal name="failed">
		<description>Emitted if an user contacts query fails</description>
	</signal>

  <method name="getUserFriends">
    <description>Starts a operation to get the user contacts.  Should be called after callbacks are setup to avoid timing issues.</description>
  </method>
</interface>
