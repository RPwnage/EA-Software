<?xml version="1.0"?>
<interface>
	<description>Information about the current XMPP connection</description>

	<property type="Boolean" name="established" readonly="true">
		<description>Indicates if a connected, logged in XMPP connection is established</description>
	</property>

	<signal name="changed">
		<description>Emitted when any property of the connection changes</description>
	</signal>
</interface>
