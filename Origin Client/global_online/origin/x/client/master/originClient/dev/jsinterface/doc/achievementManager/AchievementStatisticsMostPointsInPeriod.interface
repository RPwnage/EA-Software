<?xml version="1.0"?>
<interface>
	<description>
		This object is used as a return value for the Achievement Statistics object
	</description>

  <property name="when" type="Date">
    <description>
      The first day of the period when the score was achieved. 
    </description>
  </property>

  <property name="howMuch" type="Integer">
    <description>
      The value of the score achieved in the period. This can be Xp, Rp, or Counts.
    </description>
  </property>
  
</interface>
