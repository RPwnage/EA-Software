
/* DO NOT EDIT THIS - YOUR CHANGES WILL BE REPLACED AT RUNTIME */

.Origin--UIToolkit--OriginMultiLineEdit {
    color: transparent;
}

Origin--UIToolkit--OriginMultiLineEdit * {
    font-family: "__FONT_FAMILY__";
    font-size: __FONT_SIZE__;
    font-weight: __FONT_WEIGHT__;
}

.Origin--UIToolkit--OriginMultiLineEdit[background=true] {
    border-image: url(:/OriginCommonControls/OriginSingleLineEdit/inputbox_off.png) 3 4 4 3 stretch stretch;
    border-width: 3px 4px 4px 3px;
    padding-right: -2px;
    padding-bottom: -2px;
}

Origin--UIToolkit--OriginMultiLineEdit:disabled {
    color: #909090;
}

Origin--UIToolkit--OriginMultiLineEdit:disabled[background=true] {
    border-image: url(:/OriginCommonControls/OriginSingleLineEdit/inputbox_disabled.png) 4 3 2 3 stretch stretch;
    border-width: 4px 3px 2px 3px;
}

.Origin--UIToolkit--OriginMultiLineEdit[childFocus=true][background=true] {
    border-image: url(:/OriginCommonControls/OriginSingleLineEdit/inputbox_on.png) 3 4 4 3 stretch stretch;
    border-width: 3px 4px 4px 3px;
}
