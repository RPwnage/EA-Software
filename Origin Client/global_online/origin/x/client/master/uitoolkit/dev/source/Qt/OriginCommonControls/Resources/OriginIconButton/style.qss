
/* DO NOT EDIT THIS - YOUR CHANGES WILL BE REPLACED AT RUNTIME */


Origin--UIToolkit--OriginIconButton {
	border-image: url(:/OriginCommonControls/OriginIconButton/iconbutton_normal.png) 2 3 4 3 stretch stretch;
	border-width: 2px 3px 4px 3px;
	min-width: 20px;
	max-width: 20px;
	min-height: 20px;
	max-height: 20px;
}
	
Origin--UIToolkit--OriginIconButton:disabled {
	border-image: url(:/OriginCommonControls/OriginIconButton/iconbutton_disabled.png) 2 3 4 3 stretch stretch;
}

Origin--UIToolkit--OriginIconButton:hover {
    border-image: url(:/OriginCommonControls/OriginIconButton/iconbutton_hover.png) 2 3 4 3 stretch stretch;
}

/* needs to come AFTER hover */
Origin--UIToolkit--OriginIconButton:pressed  {
	border-image: url(:/OriginCommonControls/OriginIconButton/iconbutton_pressed.png) 2 3 4 3 stretch stretch; 
}
