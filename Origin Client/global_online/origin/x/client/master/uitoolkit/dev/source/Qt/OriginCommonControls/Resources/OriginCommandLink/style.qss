
/* DO NOT EDIT THIS - YOUR CHANGES WILL BE REPLACED AT RUNTIME */


Origin--UIToolkit--OriginCommandLink
{
	border-image: url(:/OriginCommonControls/OriginCommandLink/commandlink_normal.png) 2 5 4 2 stretch stretch;
	border-width: 2px 5px 4px 2px;
	padding: 0px 0px 0px 0px;
	margin: 0px 0px 0px 0px;
}

Origin--UIToolkit--OriginCommandLink:disabled {
	border-image: url(:/OriginCommonControls/OriginCommandLink/commandlink_normal.png) 2 5 4 2 stretch stretch;
}

Origin--UIToolkit--OriginCommandLink:hover {
	border-image: url(:/OriginCommonControls/OriginCommandLink/commandlink_hover.png) 2 5 4 2 stretch stretch;
}

/* needs to come AFTER hover */
Origin--UIToolkit--OriginCommandLink:pressed  {
	border-image: url(:/OriginCommonControls/OriginCommandLink/commandlink_down.png) 2 5 4 2 stretch stretch;
}

