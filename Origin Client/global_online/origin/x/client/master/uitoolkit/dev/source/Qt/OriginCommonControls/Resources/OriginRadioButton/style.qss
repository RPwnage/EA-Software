
/* DO NOT EDIT THIS - YOUR CHANGES WILL BE REPLACED AT RUNTIME */


Origin--UIToolkit--OriginRadioButton {
   	color: #424242;
    spacing: 2px;

    font-family: "__FONT_FAMILY__";
    font-size: __FONT_SIZE__;
    font-weight: __FONT_WEIGHT__;
}

Origin--UIToolkit--OriginRadioButton:disabled {
   	color: #b0b0b0;
}

Origin--UIToolkit--OriginRadioButton::indicator {
    width: 26px;
    height: 26px;
}

/* matches the unchecked state of the indicator subcontrol of QRadioButton */
Origin--UIToolkit--OriginRadioButton::indicator::unchecked {
    image: url(:/OriginCommonControls/OriginRadioButton/normal_unchecked.png);
}

Origin--UIToolkit--OriginRadioButton::indicator:unchecked:hover {
    image: url(:/OriginCommonControls/OriginRadioButton/normal_unchecked.png);
}

Origin--UIToolkit--OriginRadioButton::indicator::checked {
    image: url(:/OriginCommonControls/OriginRadioButton/normal_checked.png);
}

Origin--UIToolkit--OriginRadioButton::indicator:checked:hover {
    image: url(:/OriginCommonControls/OriginRadioButton/normal_checked.png);
}

Origin--UIToolkit--OriginRadioButton::indicator:checked:focus {
    image: url(:/OriginCommonControls/OriginRadioButton/focused_checked.png);
}

Origin--UIToolkit--OriginRadioButton::indicator:unchecked:focus {
    image: url(:/OriginCommonControls/OriginRadioButton/focused_unchecked.png);
}

Origin--UIToolkit--OriginRadioButton::indicator:checked:pressed {
    image: url(:/OriginCommonControls/OriginRadioButton/pressed_checked.png);
}

Origin--UIToolkit--OriginRadioButton::indicator:unchecked:pressed {
    image: url(:/OriginCommonControls/OriginRadioButton/focused_unchecked.png);
}

Origin--UIToolkit--OriginRadioButton::indicator:checked:disabled {
    image: url(:/OriginCommonControls/OriginRadioButton/disabled_checked.png);
}

Origin--UIToolkit--OriginRadioButton::indicator:unchecked:disabled {
    image: url(:/OriginCommonControls/OriginRadioButton/disabled_unchecked.png);
}

Origin--UIToolkit--OriginRadioButton::indicator:checked[validationStatus="3"] 
{
   image: url(:/OriginCommonControls/OriginRadioButton/error_checked.png);
}

Origin--UIToolkit--OriginRadioButton::indicator:unchecked[validationStatus="3"]
{
   image: url(:/OriginCommonControls/OriginRadioButton/error_unchecked.png);
}

Origin--UIToolkit--OriginRadioButton::indicator:checked:disabled[validationStatus="3"] 
{
   image: url(:/OriginCommonControls/OriginRadioButton/disabled_checked.png);
}

Origin--UIToolkit--OriginRadioButton::indicator:unchecked:disabled[validationStatus="3"]
{
   image: url(:/OriginCommonControls/OriginRadioButton/disabled_unchecked.png);
}

Origin--UIToolkit--OriginRadioButton::indicator:unchecked:focus[validationStatus="3"] {
    image: url(:/OriginCommonControls/OriginRadioButton/focused_unchecked.png);
}

Origin--UIToolkit--OriginRadioButton::indicator:checked:focus[validationStatus="3"] {
    image: url(:/OriginCommonControls/OriginRadioButton/focused_checked.png);
}
