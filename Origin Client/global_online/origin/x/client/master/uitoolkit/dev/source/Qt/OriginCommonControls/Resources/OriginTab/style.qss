
/* DO NOT EDIT THIS - YOUR CHANGES WILL BE REPLACED AT RUNTIME */

Origin--UIToolkit--OriginTab {
    background: transparent;
	border: none;
	border-image: url(:/OriginCommonControls/OriginTab/tab_normal.png) 5 5 2 5;
	border-width: 5 5 2 5;
	min-height: 27px;
	max-height: 27px;
}

Origin--UIToolkit--OriginTab[state="normal"] {
	border-image: url(:/OriginCommonControls/OriginTab/tab_normal.png) 5 5 2 5;
	border-width: 5 5 2 5;
}

Origin--UIToolkit--OriginTab[state="hover"] {
	border-image: url(:/OriginCommonControls/OriginTab/tab_hover.png) 5 5 2 5;
	border-width: 5 5 2 5;
}

Origin--UIToolkit--OriginTab[state="active"] {
	border-image: url(:/OriginCommonControls/OriginTab/tab_active.png) 5 5 2 5;
	border-width: 5 5 2 5;
}

Origin--UIToolkit--OriginTab QPushButton#btnClose {
	border: none;
	background-image: url(:/OriginCommonControls/OriginTab/tab_close_normal.png);
	min-width: 12px;
	max-width: 12px;
	min-height: 12px;
	max-height: 12px;
}

Origin--UIToolkit--OriginTab QPushButton#btnClose:hover {
	border: none;
	background-image: url(:/OriginCommonControls/OriginTab/tab_close_hover.png);
	min-width: 12px;
	max-width: 12px;
	min-height: 12px;
	max-height: 12px;
}

Origin--UIToolkit--OriginTab QPushButton[state="pressed"]#btnClose {
	border: none;
	background-image: url(:/OriginCommonControls/OriginTab/tab_close_pressed.png);
	min-width: 12px;
	max-width: 12px;
	min-height: 12px;
	max-height: 12px;
}