
/* DO NOT EDIT THIS - YOUR CHANGES WILL BE REPLACED AT RUNTIME */


Origin--UIToolkit--OriginSingleLineEdit{
    border-image: url(:/OriginCommonControls/OriginSingleLineEdit/inputbox_off.png) 4 3 2 3 stretch stretch;
	border-width: 4px 3px 2px 3px;
   	color: #3e3d3f;
	min-height: 22px;
	max-height: 22px;
	min-width: 40px;
	padding: 0px 5px 0px 5px;

    font-family: "__FONT_FAMILY__";
    font-size: __FONT_SIZE__;
    font-weight: __FONT_WEIGHT__;
}

Origin--UIToolkit--OriginSingleLineEdit:disabled {
    border-image: url(:/OriginCommonControls/OriginSingleLineEdit/inputbox_disabled.png) 4 3 2 3 stretch stretch;
    border-width: 4px 3px 2px 3px;
	color: #909090;
}

Origin--UIToolkit--OriginSingleLineEdit:focus {
    border-image: url(:/OriginCommonControls/OriginSingleLineEdit/inputbox_on.png) 4 3 2 3 stretch stretch;
    border-width: 4px 3px 2px 3px;
}

Origin--UIToolkit--OriginSingleLineEdit:!focus[validationStatus="3"]
{
    border-image: url(:/OriginCommonControls/OriginSingleLineEdit/textEdit_invalid.png) 4 3 2 3 stretch stretch;
    border-width: 4px 3px 2px 3px;
}

Origin--UIToolkit--OriginSingleLineEdit:focus[validationStatus="3"]
{
    border-image: url(:/OriginCommonControls/OriginSingleLineEdit/textEdit_invalid_focused.png) 4 3 2 3 stretch stretch;
    border-width: 4px 3px 2px 3px;
}

Origin--UIToolkit--OriginSingleLineEdit:!focus[validationStatus="2"]
{
    border-image: url(:/OriginCommonControls/OriginSingleLineEdit/textEdit_valid.png) 4 3 2 3 stretch stretch;
    border-width: 4px 3px 2px 3px;
}

Origin--UIToolkit--OriginSingleLineEdit:focus[validationStatus="2"]
{
    border-image: url(:/OriginCommonControls/OriginSingleLineEdit/textEdit_invalid_focused.png) 4 3 2 3 stretch stretch;
    border-width: 4px 3px 2px 3px;
}

Origin--UIToolkit--OriginSingleLineEdit:read-only {
    border-image: url(:/OriginCommonControls/OriginSingleLineEdit/inputbox_disabled.png) 4 3 2 3 stretch stretch;
    border-width: 4px 3px 2px 3px;
	color: #909090;
}

Origin--UIToolkit--OriginSingleLineEdit:read-only:focus {
    border-image: url(:/OriginCommonControls/OriginSingleLineEdit/inputbox_readonly_focused.png) 4 3 2 3 stretch stretch;
    border-width: 4px 3px 2px 3px;
	color: #909090;
}
