QStackedWidget {
	background: transparent;
}

QTabBar {
	border-bottom: 1px solid #B1B1B1;
}

QTabBar::tab {
	border: none;
	background: transparent;
	border-bottom-left-radius: 0px;
	border-bottom-right-radius: 0px;
	min-height: 27px;
	max-height: 27px;
	padding-top: 0px;
	padding-right: 0px;
	padding-bottom: 0px;
	padding-left: 0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 0px;
}

QTabBar::scroller {
    width: 0px;
}
