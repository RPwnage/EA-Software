
/* DO NOT EDIT THIS - YOUR CHANGES WILL BE REPLACED AT RUNTIME */


Origin--UIToolkit--OriginCheckButton
{
	color: #424242;
    spacing: 4px;

    font-family: "__FONT_FAMILY__";
    font-size: __FONT_SIZE__;
    font-weight: __FONT_WEIGHT__;
}

Origin--UIToolkit--OriginCheckButton:disabled {
   	color: #b0b0b0;
}

Origin--UIToolkit--OriginCheckButton::indicator {
    width: 22px;
    height: 21px;
}

Origin--UIToolkit--OriginCheckButton::indicator:unchecked {
    image: url(:/OriginCommonControls/OriginCheckButton/checkbox_off.png);
}

Origin--UIToolkit--OriginCheckButton::indicator:unchecked:focus {
    image: url(:/OriginCommonControls/OriginCheckButton/checkbox_off_focused.png);
}

Origin--UIToolkit--OriginCheckButton::indicator:checked {
    image: url(:/OriginCommonControls/OriginCheckButton/checkbox_on.png);
}

Origin--UIToolkit--OriginCheckButton::indicator:checked:focus {
    image: url(:/OriginCommonControls/OriginCheckButton/checkbox_on_focused.png);
}

Origin--UIToolkit--OriginCheckButton::indicator:disabled
{
    image: url(:/OriginCommonControls/OriginCheckButton/checkbox_disabled.png);
}

Origin--UIToolkit--OriginCheckButton::indicator:disabled:checked
{
  image: url(:/OriginCommonControls/OriginCheckButton/checkbox_disabled_checked.png);
}

Origin--UIToolkit--OriginCheckButton::indicator:checked[validationStatus="3"] 
{
   image: url(:/OriginCommonControls/OriginCheckButton/checkbox_on_error.png);
}

Origin--UIToolkit--OriginCheckButton::indicator:unchecked[validationStatus="3"]
{
   image: url(:/OriginCommonControls/OriginCheckButton/checkbox_off_error.png);
}

Origin--UIToolkit--OriginCheckButton::indicator:checked:disabled[validationStatus="3"] 
{
   image: url(:/OriginCommonControls/OriginCheckButton/checkbox_disabled_checked.png);
}

Origin--UIToolkit--OriginCheckButton::indicator:unchecked:disabled[validationStatus="3"]
{
   image: url(:/OriginCommonControls/OriginCheckButton/checkbox_disabled.png);
}

Origin--UIToolkit--OriginCheckButton::indicator:unchecked:focus[validationStatus="3"] {
    image: url(:/OriginCommonControls/OriginCheckButton/checkbox_off_focused.png);
}

Origin--UIToolkit--OriginCheckButton::indicator:checked:focus[validationStatus="3"] {
    image: url(:/OriginCommonControls/OriginCheckButton/checkbox_on_focused.png);
}
