
/* DO NOT EDIT THIS - YOUR CHANGES WILL BE REPLACED AT RUNTIME */


Origin--UIToolkit--OriginPushButton {
	border-image: url(:/OriginCommonControls/OriginPushButton/ebisupushbutton_tertiary_normal.png) 9 7 12 6 stretch stretch;
	border-width: 9px 7px 12px 6px;
	padding: 0px 16px 0px 16px;
	min-width: 50px;
	min-height: 20px;
	max-height: 20px;
	color: #404040;
	outline:none;
	
    font-family: "__FONT_FAMILY__";
    font-size: __FONT_SIZE__;
    font-weight: __FONT_WEIGHT__;
}
	
Origin--UIToolkit--OriginPushButton:disabled {
	border-image: url(:/OriginCommonControls/OriginPushButton/ebisupushbutton_tertiary_disabled.png) 9 7 12 6 stretch stretch;
	color: #808080;
}

Origin--UIToolkit--OriginPushButton:hover {
    border-image: url(:/OriginCommonControls/OriginPushButton/ebisupushbutton_tertiary_hover.png) 9 7 12 6 stretch stretch;
}

/* needs to come AFTER hover */
Origin--UIToolkit--OriginPushButton:pressed  {
	border-image: url(:/OriginCommonControls/OriginPushButton/ebisupushbutton_tertiary_clicked.png) 9 7 12 6 stretch stretch; 
}
