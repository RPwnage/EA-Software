
/* DO NOT EDIT THIS - YOUR CHANGES WILL BE REPLACED AT RUNTIME */

Origin--UIToolkit--OriginPushButton {
	border-image: url(:/OriginCommonControls/OriginPushButton/ebisupushbutton_secondary_normal.png) 6 7 9 6 stretch stretch;
	border-width: 6px 7px 9px 6px;
	padding: 0px 16px 0px 16px;
	min-width: 52px;
	min-height: 24px;
	max-height: 24px;
	color: #ffffff;
	outline:none;
	
    font-family: "__FONT_FAMILY__";
    font-size: __FONT_SIZE__;
    font-weight: __FONT_WEIGHT__;
}

Origin--UIToolkit--OriginPushButton:disabled {
	border-image: url(:/OriginCommonControls/OriginPushButton/ebisupushbutton_secondary_disabled.png) 6 7 9 6 stretch stretch;
	color: #efefef;
}

Origin--UIToolkit--OriginPushButton:hover {
    border-image: url(:/OriginCommonControls/OriginPushButton/ebisupushbutton_secondary_hover.png) 6 7 9 6 stretch stretch;
}

/* needs to come AFTER hover */
Origin--UIToolkit--OriginPushButton:pressed  {
	border-image: url(:/OriginCommonControls/OriginPushButton/ebisupushbutton_secondary_clicked.png) 6 7 9 6 stretch stretch; 
}
