<?xml version="1.0"?>
<StudioDocument OneInstance="false" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  
  <!-- List of platforms supported by the project -->
  <Platforms>
    <Platform Label="Win32">pc</Platform>
  </Platforms>
  
  <!-- List of configurations supported by the project -->
  <BuildTypes>
    <BuildType Label="DEBUG">-vc-dev-debug</BuildType>
    <BuildType Label="RELEASE">-vc-dev-opt</BuildType>
  </BuildTypes>
  
  <!-- List of links for the project -->
  <HtmlLinks>
    <Link Label="Jenkins">http://ebisu-build01:8080/hudson</Link>
  </HtmlLinks>

  <HtmlLinks>
    <Link Label="Origin Wiki">http://online.ea.com/documentation/display/EBI/Origin+Internal</Link>
  </HtmlLinks>
  
  <!-- Set the Perforce port to use -->
  <P4Port>p4serv1.online.ea.com:4487</P4Port>


  <!-- List of Gui elements for build options -->
  <GuiElements>

    <BuildTask Label="Perforce Sync" UseSelectedPlatform="false" UseSelectedBuildType="false" UncheckAfterBuild="true" UseFullPath="false">
     <BuildProgram>p4.exe</BuildProgram>
      <BuildOptions>
        <ListOption Label="Sync Options">
          <Item Label="Main - Normal Sync">sync //global_online/ebisu/client/main/...</Item>
          <Item Label="Main - Force Sync">sync -f //global_online/ebisu/client/main/...</Item>
        </ListOption>
        <TextBoxOption Label="ChangeList Number" ArgumentPrefix="" ArgumentSuffix="">
          <Argument>LastKnowGoodLabel</Argument>
        </TextBoxOption>
      </BuildOptions>
    </BuildTask>
  
    <BuildTask Label="Build Origin" UseSelectedPlatform="true" UseSelectedBuildType="true" UseFullPath="false">
		<BuildProgram>nant.exe</BuildProgram>
        <CommandPrefix></CommandPrefix>
		<BuildOptions>
			<CheckBoxOption Label="Clean" Checked="false">
				<CheckedArguments>clean</CheckedArguments>
			</CheckBoxOption>
			<CheckBoxOption Label="Build Code (Local)" Checked="true">
				<CheckedArguments>-D:config=${Platform}${BuildType} build</CheckedArguments>
			</CheckBoxOption>
			<CheckBoxOption Label="Build Code (Incredibuild)" Checked="true">
				<CheckedArguments>-D:config=${Platform}${BuildType} slnruntime incredibuild</CheckedArguments>
			</CheckBoxOption>
		</BuildOptions>
    </BuildTask>

    <BuildTask Label="Build EACoreCLI" UseSelectedPlatform="true" UseSelectedBuildType="true" UseFullPath="false">
      <BuildProgram>nant.exe</BuildProgram>
      <WorkingFolder>core\EACoreCLI</WorkingFolder>
      <CommandPrefix></CommandPrefix>
      <BuildOptions>
        <CheckBoxOption Label="Clean" Checked="false">
          <CheckedArguments>clean</CheckedArguments>
        </CheckBoxOption>
        <CheckBoxOption Label="Build Code" Checked="true">
          <CheckedArguments>-masterconfigfile:..\..\masterconfig.xml -D:config=${Platform}${BuildType} build</CheckedArguments>
        </CheckBoxOption>
      </BuildOptions>
    </BuildTask>

    <!-- List of application trigger -->
    
    <RunTrigger Label="P4V">
      <Program>C:\Perforce\P4v.exe</Program>
    </RunTrigger>
    
    <RunTrigger Label="VS2010">
      <Program>C:\Program Files (x86)\Microsoft Visual Studio 10.0\Common7\IDE\devenv.exe</Program>
    </RunTrigger>
    
  </GuiElements>
    
</StudioDocument>