<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup
    Condition="'$(UI_Build_RuleBeforeTargets)' == '' and '$(UI_Build_RuleAfterTargets)' == '' and '$(ConfigurationType)' != 'Makefile'">
    <UI_Build_RuleBeforeTargets>Midl</UI_Build_RuleBeforeTargets>
    <UI_Build_RuleAfterTargets>CustomBuild</UI_Build_RuleAfterTargets>
  </PropertyGroup>
  <PropertyGroup>
    <UI_Build_RuleDependsOn
      Condition="'$(ConfigurationType)' != 'Makefile'">_SelectedFiles;$(UI_Build_RuleDependsOn)</UI_Build_RuleDependsOn>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <UI_Build_Rule>
      <CommandLineTemplate>$(QtRoot)\bin\uic.exe %(FullPath) -o ui_%(Filename).h</CommandLineTemplate>
      <Outputs>ui_%(Filename).h</Outputs>
      <ExecutionDescription>Building UI File %(Filename)%(Extension)</ExecutionDescription>
      <AdditionalDependencies>$(QtRoot)\bin\uic.exe</AdditionalDependencies>
    </UI_Build_Rule>
  </ItemDefinitionGroup>
</Project>
