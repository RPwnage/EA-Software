<project default="build">
    <!-- ************************* -->
    <!-- EAConfig target overrides -->
    <!-- ************************* -->
	<optionset name="config.targetoverrides" >
		<option name="clean" value="exclude"/>
	</optionset>

	<!-- ******************* -->
	<!-- Package declaration -->
	<!-- ******************* -->
	<property name="package.OriginLegacyApp.dir"  value="@{PathGetFullPath('.')}"/>
	<property name="package.OriginLegacyApp.version"  value="@{PathGetFileName('${package.OriginLegacyApp.dir}')}"/>
	<package name="OriginLegacyApp" targetversion="${package.OriginLegacyApp.version}"/>
	
  <include file="OriginLegacyLib.xml" />

</project>
