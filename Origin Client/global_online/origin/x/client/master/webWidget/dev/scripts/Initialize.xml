<project>

    <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
    Set up properties which reference directories and executables within the
    directory structure.  
    - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<property name="package.webWidget.includedirs">
		${package.webWidget.dir}\include
	</property> 
	
	<property name="package.webWidget.libdir" value="${package.webWidget.builddir}/${config}/lib" />
	
    <fileset name="package.webWidget.libs">
		<includes name="${package.webWidget.libdir}/${lib-prefix}webWidget${lib-suffix}"/>
    </fileset>
	
</project>
