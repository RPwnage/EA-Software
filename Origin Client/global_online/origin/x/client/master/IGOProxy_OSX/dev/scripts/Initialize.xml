	<property name="package.IGOProxy_OSX.config-xcode-type" value="Release" if="${config-type} =='opt'"/>
	<property name="package.IGOProxy_OSX.bindir" value="${package.IGOProxy_OSX.dir}/build/XCodeOSX/build/${package.IGOProxy_OSX.config-xcode-type}/IGOProxy_OSX" />
	<property name="package.IGOProxy_OSX.dylib" value="${package.IGOProxy_OSX.dir}/build/XCodeOSX/build/${package.IGOProxy_OSX.config-xcode-type}/IGOLoader.dylib" />

</project>
