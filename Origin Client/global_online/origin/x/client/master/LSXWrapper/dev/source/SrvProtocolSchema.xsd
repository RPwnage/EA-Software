  <xs:simpleType name="SubscriptionStatus">
    <xs:annotation>
      <xs:documentation>These enums reflect the status the subscription can be in.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:enumeration id="UNKNOWN" value="0">
        <xs:annotation>
          <xs:documentation>When an unknown enum value is returned, or never set UNKNOWN will be returned.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration id="CREATED" value="1">
        <xs:annotation>
          <xs:documentation>The Subscription is in the created state. Not sure the client will ever see this state.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration id="ENABLED" value="2">
        <xs:annotation>
          <xs:documentation>This is the normal status when the subscription is active.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration id="EXPIRED" value="3">
        <xs:annotation>
          <xs:documentation>When the payment method lapses. The subscription will be expired.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration id="DISABLED" value="4">
        <xs:annotation>
          <xs:documentation>The subscription has been disabled by CE.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration id="CONVERTED" value="5">
        <xs:annotation>
          <xs:documentation>Not sure the client will ever see this state.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration id="PENDINGEXPIRED" value="6">
        <xs:annotation>
          <xs:documentation>The user canceled the recurring payment. The subscription will expire on the billing date.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration id="SUSPENDED" value="7">
        <xs:annotation>
          <xs:documentation>Not sure the client will ever see this state.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

