<project>
    <fragments>
      <include name="../../OriginBootstrap/dev/masterconfig_versions.xml"/>
    </fragments>

  <ondemand>true</ondemand>

  <config package="eaconfig" default="pc-vc-dev-debug" includes="pc-vc-dev-debug pc-vc-dev-opt" />

	<buildroot>build</buildroot>
  
	<globalproperties>
		incredibuild.usexge
		incredibuild.OriginLegacyApp
		nobuild.OriginLegacyApp
		eaconfig.build-MP
		jenkins
		eaconfig.rtti
		xcode-projectize
    xcode-osx-template-dir
    xcode-postprocessing-strip
    xcode-compiler
    dll-suffix
	</globalproperties>
  
  <property name="xcode-postprocessing-strip" value="false" />
  
  <!-- Override the dll-suffix so that dynamic libs on OSX end with a .dylib extension instead of .so -->
  <property name="dll-suffix" value=".dylib" readonly="true" />
</project>
