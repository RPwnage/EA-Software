<project>
    <masterversions>
    
        <!-- Compiler & SDK packages-->
        <package name="ActivePython" version="2.6.5.1-origin" />
        <package name="BugSentry" version="2.4.0-origin" />
        <package name="coreallocator" version="1.03.08" />
        <package name="DirtySDK" version="13.3.1.0.0-origin" />
        <package name="EAAssert" version="1.04.01" />
        <package name="EABase" version="2.00.40" />
        <package name="eaconfig" version="2.09.00" />
        <package name="EACallstack" version="1.20.00-origin" />
        <package name="EAIO" version="2.16.02-origin" />
        <package name="EAStdC" version="1.08.05" />
        <package name="EASTL" version="1.15.04" />
        <package name="EAThread" version="1.17.02" />
        <package name="EATrace" version="2.09.06" />
        <package name="ExceptionHandler" version="1.10.03-origin2" />
        <package name="Framework" version="3.10.00" />
        <package name="log4cxx" version="0.10.0" />
        <package name="PPMalloc" version="1.20.02" />
        
        <!-- origin packages -->
        <package name="OriginConfig" version="1.0.1" />

        <!-- Distributed build packages  -->
        <package name="IncrediBuild" version="3.61.1.1243" />
        <package name="sndbs" version="2.0.47.19" />
        <package name="snvsi" version="1.8.2" />
        <package name="vstomaketools" version="dev" />
        <package name="WindowsSDK" version="7.1.7600.0.30514"/>
        <package name="XGEAddin" version="3.0.0" />
        
        <!-- pc packages -->
        <package name="DotNet" version="4.0-1" />
        <package name="nantToVSTools" version="3.19.01" />
        <package name="SignTool" version="1.0.0" />
        <package name="VisualStudio" version="10.0.0-pr1" />
        
        <!-- mac packages -->
        <package name="osx_config" version="1.03.00" />
        <package name="UnixGCC" version="0.05.00" />
        <package name="UnixClang" version="0.03.00" />
        <package name="XcodeProjectizer" version="2.00.00-origin"/>
        
        <!-- documentation -->
        <package name="Doxygen" version="1.7.4" />
        <package name="GraphViz" version="2.6.0" />
        
    </masterversions>

    <ondemand>true</ondemand>

    <packageroots>
        <packageroot>../../../../../../packages</packageroot>
        <packageroot>../../../../../../../packages</packageroot>
        <packageroot>../../../../..</packageroot>
        <packageroot>..</packageroot>
    </packageroots>

    <config package="eaconfig" default="pc-vc-dev-debug" includes="pc-vc-dev-debug pc-vc-dev-opt" />

    <buildroot>build</buildroot>
  
    <globalproperties>
        incredibuild.usexge
        incredibuild.OriginLegacyApp
        nobuild.OriginLegacyApp
        eaconfig.build-MP
        jenkins
        eaconfig.rtti
        xcode-projectize
        xcode-osx-template-dir
        xcode-postprocessing-strip
        xcode-compiler
        dll-suffix
    </globalproperties>
  
    <property name="xcode-postprocessing-strip" value="false" />

    <!-- Override the dll-suffix so that dynamic libs on OSX end with a .dylib extension instead of .so -->
    <property name="dll-suffix" value=".dylib" readonly="true" />
</project>
