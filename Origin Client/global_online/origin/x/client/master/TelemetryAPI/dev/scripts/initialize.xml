<project>

	<!-- TelemetryAPI header files location. -->
	<property name="package.TelemetryAPI.includedirs">
		${package.TelemetryAPI.dir}/include
	</property>

	<!-- EAIO libraries location. -->
	<do if="@{PropertyExists('config')}">
		<do if="@{PropertyExists('lib-prefix')}">
			<do if="@{PropertyExists('lib-suffix')}">
				<fileset name="package.TelemetryAPI.libs">
					<includes name="${package.TelemetryAPI.builddir}/${config}/lib/${lib-prefix}TelemetryAPI${lib-suffix}" />
				</fileset>
			</do>
		</do>
	</do>
</project>
