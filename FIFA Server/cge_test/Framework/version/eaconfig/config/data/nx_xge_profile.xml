<Profile FormatVersion="1">
  <Tools>
    <!-- This is a sample IncrediBuild profile.xml that is used to control
             how IncrediBuild distributes the build tasks from make. -->

    <!-- standard build tools -->
    <Tool Filename="gnumake" AllowIntercept="true" SuccessExitCodes="0" DeriveCaptionFrom="firstparam" />
    <Tool Filename="vsimake" AllowRemote="false" SingleInstancePerAgent="true" AllowIntercept="true" SuccessExitCodes="0" DeriveCaptionFrom="firstparam"/>
    <Tool Filename="vsimake_wrapper" AllowRemote="false" SingleInstancePerAgent="true" AllowIntercept="true" SuccessExitCodes="0" DeriveCaptionFrom="firstparam"/>
    <Tool Filename="nant" AllowRemote="false" SuccessExitCodes="0" DeriveCaptionFrom="firstparam" />

    <Tool Filename="spu-lv2-gcc" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />
    <Tool Filename="ppu-lv2-gcc" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />
    <Tool Filename="ppu-lv2-gcc-4.0.2" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" />
    <Tool Filename="ppu-lv2-g++" AllowRemote="true" SuccessExitCodes="0" />
    <Tool Filename="cc1plus" AllowRemote="true" SuccessExitCodes="0" />
    <Tool Filename="vsisnarl" AllowRemote="false" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />
    <Tool Filename="tibsqlchecker" AllowRemote="false" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />
    <Tool Filename="timex" AllowIntercept="true" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />

    <!-- Special linker that is a copy of the compiler. -->
    <!-- This allows us to disable remote execution for linking only. -->
    <Tool Filename="ppu-lv2-gcc.LINKER" AllowRemote="false" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />
    <Tool Filename="spu-lv2-gcc.LINKER" AllowRemote="false" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />

    <!-- compilers required for profile builds -->
    <Tool Filename="ppu-lv2-gcc.GENUINE" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />
    <Tool Filename="ppu-lv2-gcc.GENPROF" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />
    <Tool Filename="PS3ProfPP" AllowRemote="true" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />

    <!-- SNC build tools -->
    <Tool Filename="ps3ppusnc" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />
    <Tool Filename="ps3ppusnc_noremote" AllowRemote="false" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam"/>
    <Tool Filename="ps3ppuld" AllowRemote="false" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />
    <Tool Filename="ps3snarl" AllowRemote="false" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />

    <!--
    Guarantee that echo is being executed on local machine in the case that cygwin is in PATH env variable.
    Otherwise builds will fail on machines that don't have cygwin installed
    -->
    <Tool Filename="echo" AllowRemote="false"/>

    <!-- DOS/SH commands that need to be distributable so the build doesn't choke -->
    <Tool Filename="if" AllowRemote="true"/>
    <Tool Filename="mkdir" AllowRemote="true"/>
    <Tool Filename="set" AllowRemote="true"/>
    <Tool Filename="rm" AllowRemote="true"/>
    <Tool Filename="move" AllowRemote="true"/>
    <Tool Filename="xcopy" AllowRemote="true" SuccessExitCodes="0,1,2,4"/>

    <!-- Build-tool wrappers -->
    <Tool Filename="compiler_wrapper" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false"  DeriveCaptionFrom="lastparam"/>
    <Tool Filename="buildtool_wrapper" AllowRemote="false" SuccessExitCodes="0" DeriveCaptionFrom="lastparam"/>

    <!-- Visual Studio PC and Xenon tools -->
    <Tool Filename="cl" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" VCCompiler="true" DeriveCaptionFrom="lastparam" />
    <Tool Filename="ml" AllowRemote="true" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />
    <Tool Filename="lib" AllowRemote="false" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />
    <Tool Filename="link" AllowRemote="false" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />
    <Tool Filename="rc" AllowRemote="true" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />
    <Tool Filename="mt" AllowRemote="true" SuccessExitCodes="0,1090650113" DeriveCaptionFrom="lastparam" />

    <!-- Wii REV -->
    <Tool Filename="mwcceppc" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />
    <Tool Filename="mwasmeppc" AllowRemote="true" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />
    <Tool Filename="mwldeppc" AllowRemote="false" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />

    <!-- GHS -->
    <!-- Compiler/linker/archiver are the same name (need to copy and paste to allow for remote execution of compiler -->
    <Tool Filename="cxppc" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />
    <Tool Filename="asppc" AllowRemote="true" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />
    <Tool Filename="cxppclib" AllowRemote="false" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />
    <Tool Filename="cxppclink" AllowRemote="false" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />

      <!-- Android -->
    <Tool Filename="arm-eabi-ar" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />
    <Tool Filename="arm-eabi-g++" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />
    <Tool Filename="arm-eabi-gcc" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />

    <Tool Filename="arm-linux-androideabi-ar" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />
    <Tool Filename="arm-linux-androideabi-g++" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />
    <Tool Filename="arm-linux-androideabi-gcc" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />

    <!-- PSP2 -->
    <Tool Filename="arm-eabi-as" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />
    <Tool Filename="psp2snc" AllowRemote="true" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />
    <Tool Filename="psp2snarl" AllowRemote="false" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />
    <Tool Filename="psp2ld" AllowRemote="false" SuccessExitCodes="0" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />

    <!-- NX32 -->
    <Tool Filename="armv7l\bin\clang++.exe" AllowRemote="true" SuccessExitCodes="0" DeriveCaptionFrom="lastparam" />

    <!-- Additional Tools -->
    <Tool Filename="isFileOlder" AllowRemote="false" SuccessExitCodes="0,1" AllowRestartOnLocal="false" DeriveCaptionFrom="lastparam" />
    <Tool Filename="GenIncludePathInfo" AllowRemote="false" SuccessExitCodes="0" DeriveCaptionFrom="lastparam"/>
    <Tool Filename="python" AllowRemote="false" SuccessExitCodes="0" DeriveCaptionFrom="firstparam" />
  </Tools>
</Profile>
