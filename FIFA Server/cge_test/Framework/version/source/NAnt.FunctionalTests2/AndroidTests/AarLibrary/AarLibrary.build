<project default="regress">
	<package name="AarLibrary"/>

	<AndroidAar name="AarLibrary" />

	<target name="regress">
		<property name="output-aar" value="@{PathCombine('@{GetModuleOutputDirEx('lib', 'AarLibrary', 'AarLibrary', 'runtime')}', 'AarLibrary.aar')}"/>
		<delete file="${output-aar}"/>

		<call target="gensln"/>
		<call target="buildsln"/>

		<fail unless="@{FileExists(${output-aar})}"/>
	</target>
</project>