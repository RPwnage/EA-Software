<project xmlns="schemas/ea/framework3.xsd">
  <!-- Lite mode does not build stress module by default which depends on all the components -->
  <property name="exclude.modules" value="stress" if="${blazeserver.lite??false}"/>

  <property name="exclude.modules">
    ${property.value}
    <!-- Specify modules to exclude here-->    
    bt3
    stress
    rpcinfo
    rdirtransform
    email2hash
    firedrill
    heat2logreader
    memtest
    obfuscate
  </property>
</project>
