<project xmlns="schemas/ea/framework3.xsd">

  <target name="create-prebuilt-package">

    <property name="__eaconfig.build.groups" value="${eaconfig.create-prebuilt.buildgroups??runtime tool example test}" />
    <call target="eaconfig-build-graph-all"/>

    <create-prebuilt-package 
        prebuilt-output-dir="${eaconfig.create-prebuilt.outputdir??${nant.project.buildroot}\Prebuilt}"
        test-for-boolean-properties="${eaconfig.create-prebuilt.test-for-boolean-properties??}"
        extra-token-mappings="${eaconfig.create-prebuilt.extra-token-mappings??}"
        redirect-includedirs="${eaconfig.create-prebuilt.redirect-includedirs??false}"
        redirect-includedirs-for-rootdirs="${eaconfig.create-prebuilt.redirect-includedirs-for-rootdirs??}"
        preserve-properties="${eaconfig.create-prebuilt.preserve-properties??}"
        new-version-suffix="${eaconfig.create-prebuilt.new-version-suffix??}"
        out-referenced-source-paths-property="${eaconfig.create-prebuilt.out-referenced-source-paths-property??}"
    />

  </target>

</project>
