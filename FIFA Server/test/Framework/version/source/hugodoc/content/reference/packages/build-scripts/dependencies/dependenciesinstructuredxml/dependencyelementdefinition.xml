<dependencies>
  <!-- auto dependencies are 'build' type if module has link, or 'use' type if module does not have a link.-->
  <auto
      interface="true/false"
      link="true/false"
      copylocal="true/false"
      internal="true/false"
      if="true/false"
      unless="true/false"
      append="true/false">
    
    [Package]/[group]/[module]
    . . . . .
  </auto>
  <!-- use dependencies don't build -->
  <use>
    Same as above
  </use>
  <build>
    Same as above
  </build>
  <!-- 'interface' flag is preset to true, and 'link' to false. equivalent to <use interface="true" link="false"> -->
  <interface>
    Same as above
  </interface>
  <!-- 'interface' flag is preset to false, and 'link' to true. equivalent to <build interface="true" link="false"> -->
  <link>
    Same as above
  </link>
</dependencies>
