group rpcdependencies : servercommon;

listDependencies() ::= <<
GEN_<it.Name; format="tl"> := \\
    <it.FileList; separator=" \\\n">
>>

fileGroupList(groupList) ::= <<
<groupList:listDependencies(); separator="\n\n">

SRC_BLAZERPCCOMPONENT :=

<groupList:{<it.FileList; separator=" ">}; separator=" "> : blazerpccomponent.rpc
>>
