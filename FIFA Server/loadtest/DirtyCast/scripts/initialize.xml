<project>
    <!--
        Dependent packages may use ${package.DirtyCast.includedirs} in their 
        includedirs element of build tasks.
    -->
    <publicdata packagename="DirtyCast">
        <module name="eafn">
            <includedirs>
                ${package.DirtyCast.dir}/eafn
                ${package.DirtyCast.dir}/logger
            </includedirs>
            <libs/> <!-- use default path and name -->
        </module>
        <module name="shared">
            <includedirs>
                ${package.DirtyCast.dir}/shared
            </includedirs>
            <libs/> <!-- use default path and name -->
        </module>
        <module name="version">
            <libs/> <!-- use default path and name -->
        </module>
    </publicdata>
</project>
