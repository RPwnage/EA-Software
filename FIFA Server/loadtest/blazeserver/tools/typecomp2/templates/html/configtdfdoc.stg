group configtdfdoc : configtdfdocbase;

masterComponent(fileNode, component) ::= <<>>
slaveComponent(fileNode, component) ::= <<
$standardFileHeader()$
<head>
<title>Types ($component.Name$)</title>
<meta http-equiv="Content-type" content="text/html; charset=utf-8" />
<link rel="stylesheet" type="text/css" name="cfgdoc" href="../wadl.css" />
</head>

<h1 class="pagetitle">$component.Name$ Configuration</h1>
<a name="Overview"></a><h1> <span class="mw-headline">Overview</span></h1>
This document describes the configuration file for the component $component.Name$.

<table style="width: 488px; height: 173px;" id="toc" class="toc" summary="Contents">
<tbody>
<tr>
<td>
<div id="toctitle"><h2>Contents</h2></div>
<ul>

$if(component.configurationType)$
<li class="toclevel-1"><a href="#Overview"><span class="toctext">Overview</span></a>
</li>
<li class="toclevel-1"><a href="#TDF_Types"><span class="toctext">Configuration Types</span></a>
<ul>
$tocHelper(node=component.configurationType.ActualSymbol)$
</ul>
</li>
$else$
Component $component.Name$ does not have configuration.
$endif$
$if(component.preconfigurationType)$
<li class="toclevel-1"><a href="#Overview"><span class="toctext">Overview</span></a>
</li>
<li class="toclevel-1"><a href="#TDF_Types"><span class="toctext">Preconfiguration Types</span></a>
<ul>
$tocHelper(node=component.preconfigurationType.ActualSymbol)$
</ul>
</li>
$else$
Component $component.Name$ does not have preconfiguration.
$endif$
</ul>
</td>
</tr>
</tbody>
</table>
$if(component.configurationType)$
<a name="TDF_Types"></a><h1> <span class="mw-headline">Configuration Types</span></h1>
$fileNode.DefTable.Clear$
$contentHelper(node=component.configurationType.ActualSymbol)$
$endif$
$if(component.preconfigurationType)$
<a name="TDF_Types"></a><h1> <span class="mw-headline">Preconfiguration Types</span></h1>
$fileNode.DefTable.Clear$
$contentHelper(node=component.preconfigurationType.ActualSymbol)$
$endif$
</body>
$standardFileFooter()$
>>

