<project default="regress">

	<package name="AndroidTests"/>

	<AndroidApk name="JavaDependencies" gradle-directory="JavaDependencies">
		<dependencies>
			AndroidTests/JavaLibrary
		</dependencies>
	</AndroidApk>

	<JavaLibrary name="JavaLibrary" gradle-directory="JavaLibrary"/>

	<target name="regress">
        <nant buildfile="${nant.project.buildfile}" target="slnruntime msbuild">
			<property name="config" value="android-arm-clang-debug"/>
			<property name="package.configs" value="android-arm-clang-debug"/>
		</nant> 
	</target>
</project>