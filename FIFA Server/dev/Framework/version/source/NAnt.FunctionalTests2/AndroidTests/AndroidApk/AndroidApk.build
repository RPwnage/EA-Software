<project default="regress">

	<package name="AndroidApk"/>

	<AndroidApk name="AndroidApk"/>

	<target name="regress">
		<property name="output-apk" value="@{PathCombine('@{GetModuleOutputDirEx('bin', 'AndroidApk', 'AndroidApk', 'runtime')}', 'AndroidApk.apk')}"/>
		<delete file="${output-apk}"/>

		<call target="gensln"/>
		<call target="buildsln"/>

		<fail unless="@{FileExists(${output-apk})}"/>
	</target>
</project>