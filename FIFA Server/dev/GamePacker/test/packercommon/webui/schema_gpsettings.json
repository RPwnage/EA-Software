{
  "type": "object",
  "title": "Input Form (Generated)",
  "required": [
    "teamcapacity",
    "teamcount",
    "qualityfactors",
    "playerproperties",
    "playerdatafile",
    "reports"
  ],
  "properties": {
    "maxPlayers": {
      "$id": "#/properties/maxPlayers",
      "type": "integer",
      "title": "Maxplayers",
      "default": 100
    },
    "maxworkingset": {
      "$id": "#/properties/maxworkingset",
      "type": "integer",
      "title": "Maxworkingset",
      "default": 0
    },
    "teamcapacity": {
      "$id": "#/properties/teamcapacity",
      "type": "integer",
      "title": "Teamcapacity",
      "default": 10
    },
    "teamcount": {
      "$id": "#/properties/teamcount",
      "type": "integer",
      "title": "Teamcount",
      "default": 2
    },
    "playerproperties": {
      "$id": "#/properties/playerproperties",
      "type": "array",
      "title": "Playerproperties",
      "options": { "collapsed": true },
      "items": {
        "$id": "#/properties/playerproperties/items",
        "type": "string",
        "title": "#",
        "default": "",
        "examples": [
          "ued/SkillA",
          "ping/SJC"
        ]
      }
    },
    "playerdatafile": {
      "$id": "#/properties/playerdatafile",
      "type": "string",
      "title": "Playerdatafile",
      "options": { "hidden": true },
       "$comment": "CUSTOM HAND EDIT: HIDDEN FOR THIS SAMPLE: For simplicity (when implemented), your web ui tool could probably always just point to the gpplayers.json in this dir (uploads via webui_playerdata.html would simply overwrite it). Avoids multiple tracked names/files.",
      "default": "gpplayers.json"
    },
    "qualityfactormode": {
      "$id": "#/properties/qualityfactormode",
      "type": "integer",
      "title": "Qualityfactormode",
      "default": 0,
      "options": { "hidden": true },
      "$comment": "CUSTOM HAND EDIT: HIDDEN, NOT TYPICALLY NEEDED, MAY BE DELETED IN FUTURE"
    },
    "qualityfactors": {
      "$id": "#/properties/qualityfactors",
      "type": "array",
      "title": "Qualityfactors",
      "items": {
        "$id": "#/properties/qualityfactors/items",
        "type": "string",
        "title": "#",
        "default": "",
        "examples": [
          "GameSize",
          "GamePlayerPropertyAvg/0",
          "TeamSizeMin"
        ]
      }
    },
    "trace": {
      "$id": "#/properties/trace",
      "type": "array",
      "title": "Trace",
      "options": { "collapsed": true },
      "items": {
        "$id": "#/properties/trace/items",
        "type": "string",
        "title": "#",
        "default": "",
        "examples": [
          "games",
          "players",
          "algo"
        ]
      }
    },
    "reports": {
      "$id": "#/properties/reports",
      "type": "object",
      "title": "Reports",
      "options": { "collapsed": true },
      "required": [
        "report",
        "defaultdir"
      ],
      "properties": {
        "report": {
          "$id": "#/properties/reports/properties/report",
          "type": "array",
          "title": "Report",
          "options": { "collapsed": true },
          "items": {
            "$id": "#/properties/reports/properties/report/items",
            "type": "string",
            "title": "#",
            "default": "summary",
            "examples": [
              "summary",
              "games",
              "players",
              "partys",
              "workingset"
            ]
          }
        },
        "defaultdir": {
          "$id": "#/properties/reports/properties/defaultdir",
          "type": "string",
          "title": "Defaultdir",
          "default": "gpwebuilog",
          "examples": [
            "C:\\temp"
          ]
        },
        "summary": {
          "$id": "#/properties/reports/properties/summary",
          "type": "object",
          "title": "Summary",
          "required": [
            "filenoext",
            "html"
          ],
          "properties": {
            "dir": {
              "$id": "#/properties/reports/properties/summary/properties/dir",
              "type": "string",
              "title": "Dir",
              "default": ""
            },
            "filenoext": {
              "$id": "#/properties/reports/properties/summary/properties/filenoext",
              "type": "string",
              "title": "Filenoext",
              "default": "test.gpsummaryreport.log",
              "examples": [
                "test.gpsummaryreport.log"
              ]
            },
            "html": {
              "$id": "#/properties/reports/properties/summary/properties/html",
              "type": "object",
              "title": "Html",
              "required": [
                "xslfile"
              ],
              "properties": {
                "xslfile": {
                  "$id": "#/properties/reports/properties/summary/properties/html/properties/xslfile",
                  "type": "string",
                  "title": "Xslfile",
                  "default": "verticalhtml.xsl",
                  "examples": [
                    "C:\\p4\\gos-stream\\GamePacker\\test\\gtcommon\\webui\\verticalhtml.xsl"
                  ]
                }
              }
            }
          }
        },
        "games": {
          "$id": "#/properties/reports/properties/games",
          "type": "object",
          "title": "Games",
          "options": { "collapsed": true },
          "required": [
            "filenoext",
            "html"
          ],
          "properties": {
            "dir": {
              "$id": "#/properties/reports/properties/games/properties/dir",
              "type": "string",
              "title": "Dir",
              "default": ""
            },
            "filenoext": {
              "$id": "#/properties/reports/properties/games/properties/filenoext",
              "type": "string",
              "title": "Filenoext",
              "default": "test.gpgamesreport.log",
              "examples": [
                "test.gpgamesreport.log"
              ]
            },
            "html": {
              "$id": "#/properties/reports/properties/games/properties/html",
              "type": "object",
              "title": "Html",
              "required": [
                "xslfile"
              ],
              "properties": {
                "xslfile": {
                  "$id": "#/properties/reports/properties/games/properties/html/properties/xslfile",
                  "type": "string",
                  "title": "Xslfile",
                  "default": "gamesreport.xsl",
                  "examples": [
                    "C:\\p4\\gos-stream\\GamePacker\\test\\packercommon\\webui\\gamesreport.xsl"
                  ]
                }
              }
            }
          }
        },
        "players": {
          "$id": "#/properties/reports/properties/players",
          "type": "object",
          "title": "Players",
          "options": { "collapsed": true },
          "required": [
            "filenoext",
            "html"
          ],
          "properties": {
            "dir": {
              "$id": "#/properties/reports/properties/players/properties/dir",
              "type": "string",
              "title": "Dir",
              "default": ""
            },
            "filenoext": {
              "$id": "#/properties/reports/properties/players/properties/filenoext",
              "type": "string",
              "title": "Filenoext",
              "default": "test.gpplayerreport.log",
              "examples": [
                "test.gpplayerreport.log"
              ]
            },
            "html": {
              "$id": "#/properties/reports/properties/players/properties/html",
              "type": "object",
              "title": "Html",
              "required": [
                "xslfile"
              ],
              "properties": {
                "xslfile": {
                  "$id": "#/properties/reports/properties/players/properties/html/properties/xslfile",
                  "type": "string",
                  "title": "Xslfile",
                  "default": "playersreport.xsl",
                  "examples": [
                    "C:\\p4\\gos-stream\\GamePacker\\test\\packercommon\\webui\\playersreport.xsl"
                  ]
                }
              }
            }
          }
        }
      }
    }
  }
}