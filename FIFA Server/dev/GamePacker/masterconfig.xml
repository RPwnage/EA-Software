<?xml version="1.0" encoding="utf-8"?>
<project xmlns="schemas/ea/framework3.xsd">
    <masterversions>
        <package name="nantToVSTools"                   version="3.19.09" />

        <package name="vstomaketools"                   version="2.06.09" />
        <package name="DotNet"                          version="4.72" />
        <package name="WindowsSDK"                      version="10.0.19041-proxy" />

        <package name="coreallocator"                   version="1.04.02" />
        <package name="EAAssert"                        version="1.05.07" />
        <package name="EABase"                          version="2.08.04" />
        <package name="EAIO"                            version="2.23.01" />
        <package name="EASTL"                           version="3.12.01" />        <!-- depends on EAStdC -->
        <package name="EAStdC"                          version="1.23.00" />        <!-- depends on eathread -->
        <package name="EAThread"                        version="1.32.01" />
        <package name="EAMain"                          version="3.0.0" />        <!-- used only for CommandLineOption, may get rid of to avoid adding dependency -->
        
        <!-- gtest -->
        <package name="gtest"                           version="1.8.0.ea.10" />
    </masterversions>

    <config package="eaconfig" default="pc64-vc-debug" includes="pc64-vc-debug pc64-vc-opt"/>
    
    <buildroot>build</buildroot>
    <ondemand>true</ondemand>
    <ondemandroot>../../packages</ondemandroot>

    <globalproperties>
    vsversion=2017
    EASTL_SIZE_T_32BIT=1
    eaconfig.runtimeerrorchecking=off <!-- Turn of VC++ RTC1 checks because they slow down debug built runtime x2.5: https://randomascii.wordpress.com/2011/07/22/visual-c-debug-buildsfast-checks-cause-5x-slowdowns/ -->
    cc.vc.additional-options=/JMC- <!-- Turn of VC++ Just My Code because they slow down debug runtime *and* affect accuracy and precision of reported profiling data when testing packer with large populations -->
    </globalproperties>
</project>
