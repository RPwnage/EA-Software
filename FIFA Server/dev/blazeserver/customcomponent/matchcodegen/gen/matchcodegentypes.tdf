// Defines the TDF classes used in the matchcodegen component.
//  TDF classes are compiled with TypeComp, generating C++ classes that can be serialized in RPCs and Notifications.
//
// Note: see https://developer.ea.com/display/blaze/How+to+Use+Typecomp for info on the .tdf spec, as well as the compiler, TypeComp

namespace Blaze
{
namespace MatchCodeGen
{

class GenerateCodeRequest
{
	[tag="gid"]
	uint64_t mGameId;
};

class GenerateCodeResponse
{
	[tag="cod"]
	string(6) mCode;
};

class LookupGameIdRequest
{
	[tag="cod"]
	string(6) mCode;
};

class LookupGameIdResponse
{
	[tag="gid"]
	uint64_t mGameId;
};

} // MatchCodeGen
} // Blaze



