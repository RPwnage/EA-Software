{
    "api_version":"v1",
    "deploy_strategy":{
        "pod_restart_timeout":1200000000000,
        "instance_order":[{"instance_types":["configMaster"]}]
    },
    "cluster_template":{
        "redis":{"version":"v1.0.0"},
        "db":{
            "version":"v1.0.0",
            "db_hosts":[{"name":"1","size":"tiny","schemas":[{"name":"main","blaze_config_file":"framework/databases_bitc.cfg"}]}]
        },
        "blaze":{
            "version":"v1.0.0",
            "instance_layout":[
                {"name":"configMaster","count":1,"internal_ports":9,"memory_request":"100M","non_zdt":true},
                {"name":"coreSlave","count":2,"internal_ports":10,"external_ports":6,"memory_request":"200M"},
                {"name":"mmSlave","count":2,"internal_ports":9,"memory_request":"200M"},
                {"name":"gdprAuxSlave","count":1,"internal_ports":9,"memory_request":"100M","non_zdt":true},
                {"name":"searchSlave","count":2,"internal_ports":9,"memory_request":"250M"},
                {"name":"coreNSMaster","count":1,"internal_ports":9,"memory_request":"200M","non_zdt":true},
                {"name":"coreMaster","count":2,"internal_ports":9,"memory_request":"250M"},
                {"name":"grSlave","count":2,"internal_ports":9,"memory_request":"200M"},
                {"name":"pkrMaster","count":0,"internal_ports":9,"memory_request":"200M"},
                {"name":"pkrSlave","count":0,"internal_ports":9,"memory_request":"200M"}
            ],
            "mock_service_replicas":1,
            "host_type":"default"
        },
        "mesh":{
            "version": "v1.0.1",
            "enabled": false,
            "ingress":{
                "count": 2
            }
        }
    }
}
