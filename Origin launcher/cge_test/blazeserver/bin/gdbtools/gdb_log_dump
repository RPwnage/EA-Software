define dump_logger_queue
  set $queueList=1
  while ($queueList >= 0)
      set $list = (eastl::intrusive_list_base*) &Blaze::Logger::mOutputter.mActive[$queueList]
      set $curNode = $list->mAnchor.mpNext
      while ($curNode != &$list->mAnchor)
        p (('Blaze::LogEntry'*)$curNode).mData
        set $curNode = ($curNode)->mpNext
      end
      set $queueList--
  end 
end   

document dump_logger_queue
dump_logger_queue: dumps the contents of the Blaze logger queue.  Use then to get any log entries not logged to disk when a server crashes.
end
