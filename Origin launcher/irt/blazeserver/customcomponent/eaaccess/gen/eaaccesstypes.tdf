// Defines the TDF classes used in the XmsHd component
//  TDF classes are compiled with TypeComp, generating C++ classes that can be serialized in RPCs and Notifications.
//
// Note: see http://gos.online.ea.com/confluence/display/tech/Typecomp for info on the .tdf spec, as well as the compiler, TypeComp

namespace Blaze
{
namespace EaAccess
{

	[description="Structure of the attribute"]
	class EaAccessSubAttributes
	{
		[tag="pid",		description="Persona Id of user"]				int64_t 	mPersonaId;
		[tag="bgnd", 	description="Begin date of EA Access Sub"] 		string(32) 	mBeginDate;
		[tag="endd", 	description="End date of EA Access Sub"] 		string(32) 	mEndDate;
		[tag="sta", 	description="State of EA Access Sub"]			string(32) 	mState;
		[tag="err",		description="Error Code for EA Access Sub"]		string(32)	mErrorCode;
		[tag="tria",	description="Is EA Access Sub Trial"]			bool		mIsTrial;
		[tag="vali",	description="Is EA Access Sub Valid"]			bool		mIsValid;
	};
	
    [description="Request to retrieve EA Access Info for a list of users"]
    class EaAccessSubInfoRequest
    {
		[tag="pids", description="List of Persona IDs"]	list<int64_t> mPersonaIdList;
    }; 

	[description="Response of subscription request of EA Access"]
	class EaAccessSubInfoResponse
	{
		[tag="info", description="List of EA Access Sub Info"]	list<EaAccessSubAttributes> mSubInfo;
	};

} // EaAccess
} // Blaze



