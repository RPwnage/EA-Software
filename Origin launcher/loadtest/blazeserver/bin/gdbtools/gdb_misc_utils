define dumpJobQueue
    set $tail = $gSelector->mQueue->mBucket
    if ($tail != 0)
        set $curr = $gSelector->mQueue->mBucket->mNext
        while ($curr != 0)
            set $jobCurr = $curr->mJob->mNext
            set $jobTail = $curr->mJob
            while ($jobCurr != 0)
                if ($argc == 1)
                    $arg1 $jobCurr
                else
                    print $jobCurr
                    print *$jobCurr
                end
                set $jobCurr = $jobCurr->mNext
                if ($jobTail == $jobCurr)
                    set $jobCurr = 0
                end
            end 
            set $curr = $curr->mNext
            if ($curr == $tail)
                set $curr = 0
            end
        end
    else
        print "No Jobs Pending."
    end
end

document dumpJobQueue
dump_logger_queue: dumps the contents of the selector's job queue.  
end