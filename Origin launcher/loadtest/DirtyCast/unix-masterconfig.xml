<?xml version="1.0" encoding="utf-8"?>
<project>
    <masterversions>
      <!-- Build system -->
      <package name="UnixClang"      version="9.0.0-1">
        <exception propertyname="centos">
          <condition value="true" version="4.0.1" />
        </exception>
      </package>
      <package name="antlr"          version="3.5.2" />

      <!-- EA Libraries -->
      <package name="EAAssert"       version="1.05.07" />
      <package name="EABase"         version="2.09.13" />
      <package name="coreallocator"  version="1.04.02" />
      <package name="EASTL"          version="3.16.01" />
      <package name="DirtySDK"       version="15.1.7.0.1" />
      <package name="LegacyDirtySDK" version="1.0.1" />
      <package name="BlazeSDK"       version="15.1.1.10.1" />
      <package name="TelemetrySDK"   version="20.4.1.1.0" />
      <package name="PinTaxonomySDK" version="2.2.2" />
      <package name="EAStdC"         version="1.23.00" />
      <package name="EAIO"           version="2.23.01" />
      <package name="EAJson"         version="1.06.05" />
      <package name="UTFXml"         version="3.08.09" />
      <package name="EATDF"          version="15.1.1.10.1" />
      <package name="EACrypto"       version="1.03.00" />

      <!--  -->
      <package name="EAThread"       version="1.33.00" />
      <package name="jdk"            version="container-proxy-1" />

      <!-- TelemetrySDK dependency -->
      <package name="zlib"           version="1.2.11" />

      <!-- GeoIP for logparser -->
      <package name="GeoIP"          version="1.4.6-4" />

    </masterversions>

    <buildroot>build</buildroot>
    <ondemand>true</ondemand>
    <ondemandroot>../../packages</ondemandroot>

    <config package="eaconfig" default="unix64-clang-debug" includes="unix64-clang-debug unix64-clang-opt" />

    <globalproperties>
      package.DirtySDK.contribs.available="common"
      dirtysdk-distpktwindow-size
      VS.version
    </globalproperties>

    <packageroots>
        <packageroot>../</packageroot>
    </packageroots>
</project>

