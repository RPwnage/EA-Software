<?xml version="1.0" encoding="utf-8"?>
<project>
	<!-- this masterconfig is provided in order to run nant functional tests -->
	<masterversions>
		<package name="WindowsSDK" version="10.0.17763-1" uri="p4://dice-p4-one.dice.ad.ea.com:2001/SDK?cl=4484232&amp;rooted" localondemand="false"/>
	</masterversions>

	<warnings>
		<warning id="2021" as-error="false"/>
	</warnings>

	<globalproperties>
		nant.warningsaserrors="true"
	</globalproperties>

	<ondemand>true</ondemand>
	<localondemand>true</localondemand>
	<ondemandroot>${nant.drive}/packages</ondemandroot>
	<localondemandroot use-as-default="true">Local/packages</localondemandroot>

	<config package="eaconfig" default="pc64-vc-opt" includes="pc64-vc-opt pc64-vc-debug" />
</project>
