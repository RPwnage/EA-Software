<project xmlns="schemas/ea/framework3.xsd">

  <fileset name="example.libs" >
    <includes name="${package.ExamplePackage.builddir}/${config}/lib/${lib-prefix}example${lib-suffix}" />
    <includes name="${package.ExamplePackage.builddir}/${config}/lib/extra${lib-suffix}" />
  </fileset>

  <task name="ScriptInit" PackageName="ExamplePackage" >
    <IncludeDirs>
      ${package.example.dir}/include
      ${package.example.dir}/include/common
    </IncludeDirs>
    <Libs>
      example.libs
    </Libs>
  </task>
</project>