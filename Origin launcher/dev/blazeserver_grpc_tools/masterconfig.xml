<project>
  <masterversions>
    <package name="UnixGCC"           version="0.10.00"/>
    <package name="UnixClang"         version="4.0.1"/>
    <package name="UnixCrossTools"    version="2.02.00"/>
    <package name="vstomaketools"     version="2.06.09"/>
    <package name="WindowsSDK"       version="10.0.14393-proxy"/>
    <package name="zlib" version="1.2.11" />
    <package name="libopenssl"       version="1.1.1d-prebuilt" autobuildclean="false"/>

    <package name="grpc"             version="1.10.0-3-prebuilt"/>
    <package name="protobuf"         version="3.4.1"/>
    <package name="gflags"           version="2.2.1"/>
  </masterversions>

  <buildroot>build</buildroot>
  <ondemand>true</ondemand>
  <ondemandroot>../../packages</ondemandroot>
  <packageroots>
    <packageroot>..</packageroot>
  </packageroots>

  <globalproperties>
    global.defines
    cc.cpp11=true
    protobuf.use_zlib=true
  </globalproperties>

  <globaldefines>
  </globaldefines>

  <config package="eaconfig" default="pc64-vc-dev-opt" includes="pc-vc-dev-opt pc64-vc-dev-opt pc64-vc-dll-dev-opt pc-vc-dev-debug pc64-vc-dev-debug pc64-vc-dll-dev-debug"/>
</project>
