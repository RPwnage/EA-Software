<project>
  <!-- (c) Electronic Arts. All Rights Reserved. -->
  <!-- Master config for the Blaze SDK.-->

  <masterversions>

    <!-- Platform specific packages for our build tools -->
    <package name="nantToVSTools"                  version="3.19.09" />

    <package name="WindowsSDK"                     version="10.0.19041" />

    <package name="DotNetSDK"                      version="2.0-4" />
    <package name="DotNet"                         version="4.72" />
    <package name="IncrediBuild"                   version="3.61.1.1243" />
    <package name="vstomaketools"                  version="2.06.09" />
    <package name="pclintconfig"                   version="0.17.02" />
    <package name="PcLint"                         version="9.00k-nonproxy" />
    <package name="zlib"                           version="1.2.11" />

    <!-- Code generation -->
    <package name="antlr"                          version="3.5.2" />
    <package name="jdk"                            version="azul-8.0.222-pc64" />
      
    <!-- Android -->
    <package name="AndroidAPI"                     version="28" />
    <package name="AndroidEmulator"                version="1.18.00" />
    <package name="AndroidNDK"                     version="15.2.4203891" />
    <package name="AndroidSDK"                     version="100.00.00-pr1" />
    <package name="ApacheAnt"                      version="1.9.7" />
    <package name="AndroidBuildTools"              version="29.0.2" />
    <package name="AndroidPlatformTools"           version="28" />
    <package name="AndroidTools"                   version="26.1.1" />
    <package name="GradleWrapper"                  version="1.00.00-pr1" />

    <!-- Xbox One -->
    <package name="CapilanoSDK"                    version="180716-proxy">
      <exception propertyname="nonproxy">
        <condition value="true" version="180716" />
      </exception>
    </package>

    <!-- PS4 -->
    <package name="kettlesdk"                      version="8.008.781-proxy">
        <exception propertyname="nonproxy">
            <condition value="true" version="8.008.781" />
        </exception>
    </package>
    <package name="ProDG_VSI"                      version="8.00.0.3-proxy">
        <exception propertyname="nonproxy">
            <condition value="true" version="8.00.0.3" />
        </exception>
    </package>
    <package name="PlayStation4AudioInEx"          version="8.008.011" />
    <package name="PlayStation4AppContentEx"       version="8.008.011" />

    <!-- Nintendo Switch -->
    <package name="nx_config"                      version="1.01.02" />
    <package name="nxsdk"                          version="12.2.0-proxy">
        <exception propertyname="nonproxy">
            <condition value="true" version="12.2.0" />
        </exception>
    </package>
    <package name="nx_vsi"                         version="12.2.2.0-proxy">
        <exception propertyname="nonproxy">
            <condition value="true" version="12.2.2.0" />
        </exception>
    </package>

    <!-- PS5 -->
    <package name="ps5sdk"                         version="2.00.00.26-proxy" />
    <package name="Ps5VSI"                         version="2.00.0.6-proxy" />
    
    <!-- GDK -->
    <package name="GDK"                            version="201101-proxy">
        <exception propertyname="nonproxy">
            <condition value="true" version="201101" uri="p4://dice-p4-one.dice.ad.ea.com:2001/SDK/Gemini/GDK/201101?cl=9791270" />
        </exception>
    </package>

    <!-- EA packages we build with -->
    <package name="EABase"                         version="2.09.13" />
    <package name="EAAssert"                       version="1.05.07" />
    <package name="EAMessage"                      version="2.10.05" />
    <package name="EASTL"                          version="3.17.06" /> <!-- Min 3.14.02 needed by EAIO -->
    <package name="EAStdC"                         version="1.26.07" />
    <package name="EAIO"                           version="3.01.11" />
    <package name="job_manager"                    version="4.05.05" /> <!-- Needed by EAIO -->
    <package name="rwmath"                         version="3.01.01" /> <!-- Needed by job_manager -->
    <package name="EATDF"                          version="15.1.1.10.2" />
    <package name="coreallocator"                  version="1.04.02" />
    <package name="EAJson"                         version="1.06.05-maverick" /> <!-- upgrade to latest after https://jira.frostbite.ea.com/browse/SUP-1154 has been addressed -->
    <package name="UTFXml"                         version="3.08.09" />
    <package name="DirtySDK"                       version="15.1.7.0.2" />
    <package name="TelemetrySDK"                   version="20.4.1.1.0" />
    <package name="PinTaxonomySDK"                 version="2.2.2" />
    <package name="PyroSDK"                        version="15.1.1.9.3" />
    <package name="EASystemEventMessageDispatcher" version="1.05.00" />
    <package name="EAControllerUserPairing"        version="1.12.15" />
    <package name="ConsoleRemoteServer"            version="0.03.10" /> <!-- Required by EAControllerUserPairing -->
    <package name="EAController"                   version="1.09.06" />
    <package name="EAUser"                         version="1.09.08" />
    <package name="EACrypto"                       version="1.03.04" />

    <!-- Packages for EadpAuthentication -->
    <package name="continuable"                    version="4.0.0-5" />  <!-- Required by EadpFoundation -->
    <package name="function2"                      version="4.0.0-3" />   <!-- Required by EadpFoundation -->
    <package name="EadpFoundation"                 version="1.11.0" />   <!-- Required by EadpAuthentication -->
    <package name="EadpBrowserProvider"            version="1.7.0" />    <!-- Required by EadpAuthentication -->
    <package name="SteamSDK"                       version="1.49.01" />  <!-- Required by EadpAuthentication -->
    <package name="EAOrigin"                       version="1.2.0" />    <!-- Required by EadpAuthentication -->
    <package name="EadpAuthentication"             version="1.9.0" />

    <!-- Only required to build Doxygen documentation -->
    <package name="Doxygen"                        version="1.8.14-1" />
    <package name="GraphViz"                       version="2.38.0" />
    <package name="userdoc"                        version="1.00.00" />

    <!-- Packages for the samples -->
    <package name="PPMalloc"                       version="1.26.07" />
    <package name="EAThread"                       version="1.33.00" allowoverride="true" />
    <package name="EACallstack"                    version="2.02.01" allowoverride="true" />
    <package name="Speex"                          version="1.2rc1-8" allowoverride="true" />
    <package name="opus"                           version="1.00.06" />
    <!-- Nucleus SDK based login for samples -->
    <package name="NucleusSDK"                     version="20.4.1.0.0" allowoverride="true">
        <exception propertyname="package.kettlesdk.crossgen.enabled">
            <condition value="true" version="20.4.1.0.0-crossgen-custom-1" allowoverride="true" /> <!-- GOSREQ-3977: Once an official version with the ps4 cross gen support is released, use that. -->
        </exception>
    </package>

    <package name="EATrace"                        version="2.12.03" />

    <!-- Code size report tool -->
    <package name="TypeDatabase"                   version="1.04.02" />
    <package name="ActivePython"                   version="2.7.13.2716" />

    <!-- libpack package containing prebuilt libs to be linked with when mobile.useprebuiltlibpack=true -->
    <package name="BlazeSDKSamplesLibPack"         version="1.0.0" />

    <package name="StadiaSDK"                      version="1.56-proxy">
        <exception propertyname="nonproxy">
            <condition value="true" version="1.56" uri="p4://dice-p4-one.dice.ad.ea.com:2001/SDK/stadia/StadiaSDK/1.56?cl=9367309" />
        </exception>
    </package>
  </masterversions>

  <!-- Change order of package.configs so that debug builds first -->
  <!-- DirtySDK redirects debug-opt config to point to debug lib -->
  <config package="eaconfig" default="pc64-vc-debug" includes="pc64-vc-debug pc64-vc-opt pc64-vc-debug-opt pc64-vc-dll-debug pc64-vc-dll-opt pc64-vc-dll-debug-opt capilano-vc-debug capilano-vc-opt capilano-vc-debug-opt xb1gdk-vc-debug xb1gdk-vc-opt xb1gdk-vc-debug-opt xbsx-vc-debug xbsx-vc-opt xbsx-vc-debug-opt kettle-clang-debug kettle-clang-opt kettle-clang-debug-opt nx-arm64-clang-debug nx-arm64-clang-opt nx-arm64-clang-debug-opt ps5-clang-debug ps5-clang-opt ps5-clang-debug-opt stadia-clang-debug stadia-clang-opt stadia-clang-debug-opt" />

  <globalproperties>
    vsversion=2017
    vsversion.capilano=2017
    eaconfig.defines.winver=0x0602
    samples
    dirtysdk-samples
    dirtysdk_debug_enable
    dirtysdk_debug_disable
    mobile.useprebuiltlibpack=false
    blazesdk_telemetrysdk_enable
    eaconfig.build-MP=""
    nonproxy
    blazesdk_disable_default_eatdf_allocator
    NucleusSDK_NoEAWebKit=true
    NucleusSDK_UseSystemBrowser=true
    package.GDK.graphics-libs=no
    package.AndroidSDK.minimumSDKVersion=24
    <if condition="${samples} == true">
      dirtysdk-contribs=true
    </if>

    <globalproperty name="eaconfig.warningsaserrors.packages.excludes">
      EAControllerUserPairing
      NucleusSDK
      EATrace
    </globalproperty>
    EASystemEventMessageDispatcher.IncludeCompanionLibs=false
    <!-- this warning is leaked by EASTL, disable until it is fixed -->
    eaconfig.kettle-clang.warningsuppression="-Wno-misleading-indentation"
    eaconfig.ps5-clang.warningsuppression="-Wno-misleading-indentation"
  </globalproperties>
  
  <globaldefines>
    <!-- Needed for building NucleusSDK when using DirtySDK 15.1.7.0.0 -->
    DIRTYCODE_USE_NEW_CUSTOMHEADERCALLBACK=1
  </globaldefines>

  <buildroot>build</buildroot>

  <ondemand>true</ondemand>
  <ondemandroot>../../packages</ondemandroot>
  <localondemandroot>../packages</localondemandroot><!-- needed for localondemand's above -->

  <packageroots>
    <packageroot>../</packageroot>
  </packageroots>
</project>
